                    var evt = getEvent();
                    if (evt) {
                        if (evt.srcElement.tagName != "INPUT" && evt.srcElement.tagName != "TEXTAREA") return false;
                    }
                    return true;
                }
            } catch (e) { };
            var isShowStar = $("#hidIsShowStar").val();
            var userPhoto = $("#hidUserPhoto").val();
            SyncLoadHtmlContent2('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', '5915daca-08d1-475c-80d7-8e786f350bc5|%e9%ab%98%e6%95%88%e8%83%bd%e4%ba%ba%e5%a3%ab%e7%9a%84%e4%b8%83%e4%b8%aa%e4%b9%a0%e6%83%af|DocumentKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|cae89804-ea78-4402-a291-ada1a551de96|刘林平|5915daca-08d1-475c-80d7-8e786f350bc5|2abd9bbd-cfa0-4f14-8760-1d210d7ef507|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
