 Cpn = getCookie(CookieName);
 }

 var isHideZoom = false;
 var width = 1000;
 if (Cpn == "" || Cpn == "0") {
 Cpn = "1";
 }

 data.pn = Cpn;
 if (isJY == "1") {
 isHideZoom = true;
 width = 820;
 data.pn = 1;
 }

 var lastPage = 1;
 var allPage = data.pageSize;
 var m = '';
 if (m != "1") {
 try {
 window.parent.docLoaded(allPage);
 }
 catch (err) {
 //在此处理错误
 }
 }

