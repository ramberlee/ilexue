<script type="text/javascript">

 (function ($) {
 var wh = $(window).height();
 $(".el-doc-play").height(wh - 120);
 })(jQuery);

 var playerWidth = $(document).width();
 var player_doc = null;
 $('#lcPlayMain_divLabelHtmlContents').attr('style', 'max-width:' + playerWidth + 'px;height:100%');


