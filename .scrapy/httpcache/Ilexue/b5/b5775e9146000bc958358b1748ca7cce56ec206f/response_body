                    var evt = getEvent();
                    if (evt) {
                        if (evt.srcElement.tagName != "INPUT" && evt.srcElement.tagName != "TEXTAREA") return false;
                    }
                    return true;
                }
            } catch (e) { };
            var isShowStar = $("#hidIsShowStar").val();
            var userPhoto = $("#hidUserPhoto").val();
            SyncLoadHtmlContent2('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', 'bccdc56d-71f2-463e-be97-27dae484870e|%e5%83%8f%e9%ba%a6%e8%82%af%e9%94%a1%e9%a1%be%e9%97%ae%e4%b8%80%e6%a0%b7%e6%80%9d%e8%80%83|DocumentKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|cae89804-ea78-4402-a291-ada1a551de96|刘林平|bccdc56d-71f2-463e-be97-27dae484870e|b8ab0857-f3f8-4444-98b5-be7bd12e2498|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
