<script type="text/javascript">
 function show() {
 $(".box").each(function () {
 var text = $(this).html();
 var newBox = document.createElement("span");
 var btn = document.createElement("a");
 newBox.innerHTML = text.substring(0, 150);
 btn.innerHTML = text.length > 150 ? "..." + jQuery.i18n.map["common_lbl_more"] : "";
 btn.href = "javascript:void(0)";
 btn.setAttribute("class", "pl5");
 btn.onclick = function () {
 if (btn.innerHTML == "..." + jQuery.i18n.map["common_lbl_more"]) {
 btn.innerHTML = jQuery.i18n.map["common_lbl_slideup"];
 newBox.innerHTML = text;
 } else if (btn.innerHTML == jQuery.i18n.map["common_lbl_slideup"]) {
 btn.innerHTML = "..." + jQuery.i18n.map["common_lbl_more"];
 newBox.innerHTML = text.substring(0, 150);
 }
 }
 $(this).html("");
 $(this).append(newBox);
 $(this).append(btn);
 });
 }
 $(document).ready(function () {
 show();
 });
</script>
