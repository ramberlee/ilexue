<script type="text/javascript">

 //上传文档视频控件初始化
 $(document).ready(function () {
 var kngTags = ["ncc","1811"];
 var kngConfig = {};
 kngConfig.configKey = "KnowledgeConfigKey";
 kngConfig.sourceId = "501"; //金牌101 课橙201 社群30 商城401 乐才501
 kngConfig.appId = ""; //目前仅用金牌的CRM用到，别的产线现在这个值不赋
 //低版本浏览器初始化的flash地址 注：这一定是项目里面的相对路径
 kngConfig.flash_swf_url = "/Common/js/ie/Moxie.swf";
 kngConfig.auto_start = true; //是否自动上传
 //kngConfig.button_id = "btnStart";//如果自动上传为false则这个Id为"开始"按钮的Id
 kngConfig.multi_selection = true; //是否多选
 kngConfig.browse_button_id = "kngfiles"; //控件Id
 kngConfig.uplist_id = "uplist";
 //keyConfig 此配置主要用来获取 存储在存储空间的路径规则
 kngConfig.keyOrgCode = "13601042801"; //机构编号，如云学堂的机构编号为yxt
 kngConfig.keyModuleName = "knowledgefiles"; //板块名称，如知识库模块为kng
 kngConfig.keyFunctionName = "files"; //功能名称，如知识库模块下的视频功能为video
