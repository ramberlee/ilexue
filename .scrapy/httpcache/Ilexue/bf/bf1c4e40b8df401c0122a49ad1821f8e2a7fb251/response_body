 divtag.after(txt);
}


}
 
}
 });

 
 
 
 //添加大数据下拉框
 $(".tags-input").livequery(function () {


 //找到该input对应的ul
 var keyUl = $(this).parent().next();
 
 if(keyUl.length>0)
 {
 $(this).bind('input propertychange', function() {
 if($(this).val()!=null)
 {
 if($(this).val().length>=2)
 {
 keyUl.css('display','block'); 
 //拼接空的li
 $("li").remove(".ph10");
 //keyUl.style.display = "block";
 for (var i = 0; i < 1; i++) {
 var html = "<li class=\"ph10 ellipsis\" ></li>";
 keyUl.append(html);
 }
 //调用大数据接口获取关键字列表
 var arr = '{"name":"' + $(this).val() + '"}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/GetKngTagOrg",
 dataType: 'json',
 data: arr,
 cache: false,
 success: function (result) {
 if (result != "") {
 var advData = result;
 //先全部移除
 $("li").remove(".ph10");
 //succFunction(arrFir);
 for (var i = 0; i < advData.length; i++) {
 var html = "<li class=\"ph10 ellipsis hover\" >" + advData[i] + "</li>";
 keyUl.append(html);
 }

 }

 }
 });
 }
 else {
 keyUl.css('display','none'); 
 }
 }
 });
 
 
