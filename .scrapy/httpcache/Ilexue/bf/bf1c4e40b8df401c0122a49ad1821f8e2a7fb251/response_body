<script>

function handleCenter(className){
 var containHeight=document.documentElement.clientHeight-300;
	var lstImg=jQuery(className);
	for(var i=0,j=lstImg.length;i<j;i++){
		//图片大小大于可以容纳的大小
		if(lstImg.eq(i).height()>containHeight){
			lstImg.eq(i).css("top","auto");
			lstImg.eq(i).css("bottom",0);
		}
	}
}
jQuery(document).ready(function(){
handleCenter(".floatwindow_LeftCenter");
handleCenter(".floatwindow_RightCenter");
});
</script>
 
 


  






