<script type="text/javascript">
 function showEventWaitHandlePanel(a) {
 if (a == "1") {
 
 //待办事项图标闪烁用
 var txtType01 = 'SiteMessageForItemIco';
 var arr01 = '{"key":"' + txtType01 + '"}';
 //异步保存Session
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/MyIndexService.svc/SaveSessionLoginStringForItemIco",
 data: arr01,
 dataType: 'json',
 cache: false,
 success: function (result) {
 if (result == "ok") {
 $("#sWait").attr("class", "iconnotic_new");
 }
 }
 });
 }
 var Value = document.getElementById("WaitToDoMessage").value;

 if (jQuery("#dvDaibanShiyi").is(":hidden")) {

 var left = (jQuery(document).width() - 835) / 2;
 var top = (jQuery(window).height() - 403) / 2;

 jQuery("#dvDaibanShiyi").css("top", top + "px").css("left", left + "px").show();
 var a = Value.indexOf("@");
 var str;
 if (a == -1) {
 str = Value.split("|");
 }
 else {
 var str1 = Value.substring(0, a);
 str = str1.split("|");
 }
 if (str.length == 13) {
 //2&管理员|1|1|5|0|上午好|1|0|0|0|0|0|0
 //返回string类型以‘|’分割 目前包含需要13个数据
 //前面数字表示获取内容的顺序，用Str[XX]获取时需要减1
 //1：用户名（已过时，不用了） 2：用户上传的知识数（0为没有上传过）
 //3：该用户30天内上传的知识数（0为没有上传）
 //4：待审核的知识数（0为不需要审核）
 //5：已延迟的学习任务数量 6:根据当前时间获取问候语（上午，下午，晚上好）
 //7：是否上传过头像（0为未上传，1为上传）
 //8：是否安装客户端（0为未安装）
 //9：下属员工未完成的任务数量
 //10：未指派过任务的下属员工数量
 //11：下属员工30天未分享过知识的数量
 //12：下属员工7天登入的个数
 //13：下属员工未安装移动客户端的
 jQuery("#WhatTimeNow").html(str[5]);
 var isCount = 0; //判断时候需要显示全部完成的Li
 var isShowCount = 1; //判断每一个Li前面显示的数字，所以此JS内顺序不能更换
 //做一个有头有脸的人物，从头像做起。赶快上传自己的头像吧！6
 if (str[1] != "0") {
 jQuery("#FirstShareKNG").hide();
 isCount = isCount + 1;
 }
 else {
 jQuery("#FirstShareKNG").show();
 jQuery("#markcount2").html(isShowCount);
 isShowCount = isShowCount + 1;
 }
 if (str[4] == "0") {
 jQuery("#StudyPlanKNG").hide();
 isCount = isCount + 1;
 }
 else {
 jQuery("#StudyPlanKNG").show();
 jQuery("#PassStudyPlan").html(str[4]);
 jQuery("#markcount3").html(isShowCount);
 isShowCount = isShowCount + 1;
 }
 if (str[3] == "0") {
 jQuery("#WaitCkeck").hide();
 isCount = isCount + 1;
 }
 else {
 jQuery("#WaitCheckCount").html(str[3]);
 jQuery("#WaitCkeck").show();
 jQuery("#markcount4").html(isShowCount);
 isShowCount = isShowCount + 1;
 }
 //您已经一个月未分享过任何东东了，立即加入分享的队伍吧！2
// if (str[7] == "0") {
// jQuery("#InstallationApps").show();
// jQuery("#markcount6").html(isShowCount);
// isShowCount = isShowCount + 1;
// }
// else {
// jQuery("#InstallationApps").hide();
// isCount = isCount + 1;
// }
 //个学习任务未按时完成！8
 //名员工从来没有指派学习任务，立即去指派！9
 //名员工已超过30天没有分享任何知识！10
 if (str[11] == "0") {
 jQuery("#LeaderNotLoginIn").hide();
 isCount = isCount + 1;
 }
 else {
 jQuery("#LeaderNotLoginIn").show();
 jQuery("#LeaderNotLoginInText").html(str[11]);
 jQuery("#markcount10").html(isShowCount);
 isShowCount = isShowCount + 1;
 }
// if (str[12] == "0") {
// jQuery("#LeaderNotInstallationApps").hide();
// isCount = isCount + 1;
// }
// else {
// jQuery("#LeaderNotInstallationApps").show();
// jQuery("#LeaderNotInstallationAppsText").html(str[12]);
// jQuery("#markcount11").html(isShowCount);
// isShowCount = isShowCount + 1;
// }
 if (isCount != 4) {
 jQuery("#NoWaitPlanToDo").hide();
 jQuery("#ShowAttention").show();
 }
 else {
 jQuery("#NoWaitPlanToDo").show();
 }
 }
 }
 }
