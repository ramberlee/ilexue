 <script type="text/javascript">
 function showMoreMenu(sender) {
 var l = jQuery(sender).find(".blockmore .c_navmorediv1").length;
 var w = 0;
 if (l == 1) {
 r = 0;
 }

 if (l == 3 || l == 4 || l == 2) {
 r = -270;
 }

 if (l > 4) {
 w = 895;
 r = -220;
 }
 else if (l == 1) {
 r = -146;
 w = l * 220;
 }
 else {
 w = l * 222;
 }

 jQuery(sender).find(".blockmore").css("width", w + "px").css("right", r + "px");
 jQuery(sender).attr('class', 'morehover');
 }
 function hideMoreMenu(sender) {
 jQuery(sender).attr('class', 'more');
 }
 </script>
 </div>
 </div>
</div>
 
<script type="text/javascript">
 $(function () {
 var value = getCookie("XXTOWN_COOKIE_00014");
 if (value == null || value == "undefined") {
 value = "";
 }
 var isShow = value.indexOf("rolechangearea") < 0;
 if (isShow) {
 $("#rolechange").show();
 }
 else {
 $("#rolechange").attr("class", "rolechange_hidden");
 $("#rolechange").show();
 }
 });
 function changeRoleChangeView(sender) {
 var dvRoleChange = $(sender).parent();
 if (dvRoleChange.attr("class") == "rolechange") {
 // 隐藏
 dvRoleChange.attr("class", "rolechange_hidden");
 var value = getCookie("XXTOWN_COOKIE_00014");
 if (value == null || value == "undefined") {
 value = "";
 }
 value += "rolechangearea-";
 addCookie("XXTOWN_COOKIE_00014", value, 9999);
 }
 else {
 // 呈现
 dvRoleChange.attr("class", "rolechange");
 var value = getCookie("XXTOWN_COOKIE_00014");
 if (value == null || value == "undefined") {
 value = "";
 }
 value = value.replace("rolechangearea-", "");
 addCookie("XXTOWN_COOKIE_00014", value, 9999);
 }
 }
</script>
 

 
 
