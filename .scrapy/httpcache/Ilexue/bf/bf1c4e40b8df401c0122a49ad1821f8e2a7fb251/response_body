 GetDataByLabelEName("pnl_employeenotloginInfo01", "WaitToDoRelatedPage", 0, 0, "type", "NotShareKnowledgeWithNew", null);
 }
 if (a == "WeekNotLoginWithNew") {
 addFilter("dvFilter1", "603");
 jQuery("#spnotice01").html(jQuery.i18n.map['sys_lbl_sendmessageremind'])
 jQuery("#sptitle01").html(jQuery.i18n.map['sys_lbl_reminder'])
