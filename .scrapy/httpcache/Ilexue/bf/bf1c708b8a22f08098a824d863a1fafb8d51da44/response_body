        $(document).ready(function () {
            try {
                document.ondragstart = function () {
                    var evt = getEvent();
                    if (evt) {
                        if (evt.srcElement.tagName != "INPUT" && evt.srcElement.tagName != "TEXTAREA") return false;
                    }
                    return true;
                }
            } catch (e) { };
            var isShowStar = $("#hidIsShowStar").val();
            var userPhoto = $("#hidUserPhoto").val();
            SyncLoadHtmlContent2('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', 'f9ddac09-8d39-4746-a80a-26367787ef77|%e6%80%9d%e7%bb%b4%e5%af%bc%e5%9b%be|DocumentKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|cae89804-ea78-4402-a291-ada1a551de96|刘林平|f9ddac09-8d39-4746-a80a-26367787ef77|b8ab0857-f3f8-4444-98b5-be7bd12e2498|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
