        //检查是否收藏
        function CheckAddToFavorite(orgID, userID, knowledgeID, packageID, masterID, masterType, elementClientID, isInnerHtml) {
            if ($('#hfIsFavorite').val() == "1") {
                var arr = '{"knowledgeID":"' + knowledgeID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/DeleteFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")")
                        if (result.Status == "OK") {
                            $("#liFavorite").prop("class", "");
                            $("#hfIsFavorite").val("0");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_cancelcollectionsuccess']);
                        }else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            }
            else {
                var arr = '{"knowledgeID":"' + knowledgeID + '","packageID":"' + packageID + '","masterType":"' + masterType + '","masterID":"' + masterID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/AddFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")")
                        if (result.Status == "OK") {
                            $("#liFavorite").prop("class", "done");
                            $("#hfIsFavorite").val("1");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_collectionsuccess']);
                        } else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            }
        }

        //赞某对象
        function supportKng(masterIDs, type) {
            checkLogin2(function () {
                var arr = '{"masterIDs":"' + masterIDs + '","type":' + type + ',"remark":"知识学习时的赞"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/SupportObject",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        if (result == "1") {
                            $("#liSupport").removeClass().addClass("done pr");
                            $("#hfIsSupport").val("1");
                            var num = $("#praiseNum").html();
                            if (parseInt(num) < 99) {
                                $("#praiseNum").html(parseInt(num) + 1);
                                $("#praiseNum").removeClass().addClass("el-praise-num");
                            } else if (parseInt(num) == 99) {
                                $("#praiseNum").html("99+");
                            }
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_successpraise']);
                        } else if (result == "-888") {
                            showWarningMsg(jQuery.i18n.map['kng_lbl_havepraised']);
                        }
                    }
                });
            });
        }

        //返回
        function GoBack() {
            var url = $('#hfHomeURL').val();
            var m = getQueryString("m");
            if (m == 1) {
                if (url.indexOf("?") == -1) {
                    url = url + "?m=1";
                } else {
                    url = url + "&m=1";
                }
            }
            location.href = url;
        }

        ///立即开始当前的知识学习
        function StartCurStudy() {
            if (confirm(jQuery.i18n.map['kng_lbl_studytrackcomfirm'])) {
                var id = $("#hidKnowledgeID").val();
                var arr = '{"knowledgeID":"' + id + '","packageID":"89fb0321-f192-4981-938d-74e5bbf3fd8e","masterID":"","masterType":""}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/ChangeStudyKnowledge",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        var url = window.location.href;
                        if (url.indexOf("tmp=") < 0) {
                            if (url.indexOf("?") > -1) {
                                window.location.search += "&tmp=" + Date.parse(new Date());
                            } else {
                                window.location.search += "?tmp=" + Date.parse(new Date());
                            }
                        }
                        else {
                            var c = url.indexOf("tmp=");
                            var v = url.substring(c + 4, c + 17);
                            var v1 = v + 1;
                            url = url.replace(v, v1);
                            window.location.href = url;
                        }
                    }
                });
            }
        }

        //隐藏课程的提醒后记录当前隐藏的时间
        function HideCourseWarm(obj) {
            var kid = $("#hfUserKngID").val();
            var arr = '{"userKngID":"' + kid + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/AddExpiredTimeByUserKngID",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function () {
                }
            });
            $(obj).parent('.tipbox').hide();
        }

        function js_getSessionInfo() {
            var kid = 'c159b820-9494-4a4d-9b9d-233a9d980eb1';
            var userid = '49b04dcc-4796-437d-ba87-4a8eebf9a22c';
            var timestamp = new Date().valueOf();
            return "" + userid + "|" + kid + "|" + timestamp + "";    //用户ID和视频ID或课程ID,时间戳
        }

        //0：评论  1:问答    2：笔记
        function changeContentTab(obj, index) {
            if ($(obj).hasClass("active")) {

            } else {
                if (index == 0) {
                    $("#liComment").addClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").removeClass("active");
                    $("#divComment").show();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").hide();
                    var isShowStar = $("#hidIsShowStar").val();
                    var userPhoto = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', 'c159b820-9494-4a4d-9b9d-233a9d980eb1|%e5%a4%9a%e6%a0%b7%e7%9a%84Excel%e6%95%b0%e6%8d%ae%e5%8f%af%e8%a7%86%e5%8c%96|VideoKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|2408ce95-4235-440f-b1f4-ee5958cf36ea|管理员|c159b820-9494-4a4d-9b9d-233a9d980eb1|89fb0321-f192-4981-938d-74e5bbf3fd8e|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
                    var key = "MasterID|SourceID|PackageID|UserID|PageIndex|PageSize";
                    var value = "c159b820-9494-4a4d-9b9d-233a9d980eb1|c159b820-9494-4a4d-9b9d-233a9d980eb1|89fb0321-f192-4981-938d-74e5bbf3fd8e|49b04dcc-4796-437d-ba87-4a8eebf9a22c|0|10";
                    GetDataByLabelENameWithKng("dvCommentList", "KngCommentList30", 0, 0, key, value);

                } else if (index == 1) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").addClass("active");
                    $("#liNote").removeClass("active");
                    $("#divComment").hide();
                    $("#divAskQuestion").show();
                    $("#divKngNote").hide();
                    var title = $("#hidTitle").val();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divAskQuestion', 'KngViewQuestionInfo', 'imageUrl', hfimageurl);
                    GetDataByLabelENameWithKng("divQuestionList", "KngQuestionList", 0, 0, "title", title);

                } else if (index == 2) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").addClass("active");
                    $("#divComment").hide();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").show();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divKngNote', 'KngViewNoteInfo', 'KnowledgeID|KnowledgeType|ImageUrl', 'c159b820-9494-4a4d-9b9d-233a9d980eb1|VideoKnowledge|' + hfimageurl);
                    loadPropertiesForAsync("divKngNote");
                }
            }
        }

        //锚点移动
        function hashAction(id) {
            var t = $("#" + id).offset().top;
            $(window).scrollTop(t);

            $("#" + id).click();
        }

        //打赏
        function shwoReward() {
            //获取贡献者信息
            var arr = '{"knowledgeId":"c159b820-9494-4a4d-9b9d-233a9d980eb1"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngContributorView",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (result) {
                    result = eval('(' + result + ')');
                    if (result.Status == "Ok") {
                        var msgData = eval('(' + result.Data + ')');
                        if (msgData.userId == "" || msgData.userId == null) {
                            $("#divRewardFail").hide();
                            $("#divNoContributors").show();
                        } else {
                            $("#contributorImg").attr("src", msgData.headPictureUrl);
                            $("#divContributors").html(msgData.cnName);
                            $("#availablePoints").html(msgData.validPoints);
                            $("#contributeUserId").val(msgData.userId);
                            $("#divRewardFail").hide();
                            $("#divRewardTwo").hide();
                            $("#divRewardOne").show();
                        }
                    } else {
                        $("#divRewardOne").hide();
                        $("#divRewardTwo").hide();
                        $("#divRewardFail").hide();
                        $("#divNoContributors").show();
                    }
                    $("#divReward").show();
                },
                error: function (data) {

                }
            });
        }
    </script>
    <script type="text/javascript">
        (function ($) {
            var wh = $(window).height();
            $(".el-video-play").height(wh - 120);
            //$(".el-video-play").find(".el-play-area").height(wh - 120);
            if ($("#hfLectureknowledgeID").val() != "") {
                $(".el-video-play").find(".el-play-area").find(".y-container1").height(wh - 120);
            }
        })(jQuery);

        function testTransition() {
            var _style = document.createElement("div").style;
            return "transition" in _style || "mozTransition" in _style ||
					"webkitTransition" in _style;
        }

        $("#li1").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area");
            if (testTransition()) {
                elem.toggleClass("el-play-right-active");
            } else {
                if (!elem.hasClass("el-play-right-active")) {
                    elem.animate({ "margin-right": "309px" }, 200, function () {
                        elem.addClass("el-play-right-active");
                    });

                    $(".el-play-area-right").animate({ "width": "309px" }, 200);
                } else {
                    elem.animate({ "margin-right": "49px" }, 200, function () {
                        elem.removeClass("el-play-right-active");
                    });
                    $(".el-play-area-right").animate({ "width": "49px" }, 200);
                }
            }
        });

        $(".el-area-left-tag").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area-left"), target = $(".el-play-area");
            if (testTransition()) {
                $(".el-play-area-left").toggleClass("el-play-left-active");
            } else {
                if (!elem.hasClass("el-play-left-active")) {
                    target.animate({ "margin-left": "260px" }, 200, function () {
                        elem.addClass("el-play-left-active");
                    });

                    elem.animate({ "width": "260px" }, 200);
                } else {
                    target.animate({ "margin-left": "28px" }, 200, function () {
                        elem.removeClass("el-play-left-active");
                    });
                    $(".el-play-area-left").animate({ "width": "28px" }, 200);
                }
            }

        });

        
    </script>
    <script type="text/javascript">

        //下载列表的文件数量
        var downFilesCount = 0;

        //下载管理列表
        function loadDownDocList(offset) {
            var arr = '{"limit":"9","offset":"' + offset + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngDownDocList",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data != "") {
                        var result = data.datas;

                        for (var i = 0; i < result.length; i++) {

                            var html = '<li id=' + result[i].id + ' class="doc-line-list clearfix">';

                            if (result[i].fileType == "PDF") {
                                html += '<span class="icon-Knowledge-pdf pull-left">';
                            } else if (result[i].fileType == "Image") {
                                html += '<span class="icon-Knowledge-image pull-left">';
                            }

                            html += '</span><div class="list-body pull-left ml10 mr20"><div class="ellipsis text-normal hand font-size-13" title="' + result[i].knowledgeName + '">' + result[i].knowledgeName + '</div><div class="font-size-13 text-grey clearfix el-list-date">' + result[i].updateDate + '</div></div>';

                            if (result[i].isDownload == "1") {    //是否已下载 0：未下载 1：已下载 
                                html += '<div class="pull-left font-size-13 clearfix el-download-status zhclass"><div class="el-downloading"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div><div class="text-grey el-have-downloaded">(' + jQuery.i18n.map['lbl_msg_download'] + ')</div></div>';    //已下载
                            } else {
                                if (result[i].status == "0") {    //状态 0：处理中 1：处理完成 2:处理失败
                                    html += '<div class="text-grey font-size-13 pull-left clearfix being-processed zhclass">' + jQuery.i18n.map['lbl_msg_processing'] + '</div>';    //处理中
                                } else if (result[i].status == "1") {
                                    html += '<div class="text-grey font-size-13 pull-left el-downloading clearfix zhclass"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div>';
                                } else {
                                    html += '<div class="font-size-13 pull-left clearfix text-red zhclass">' + jQuery.i18n.map['lbl_msg_processfailed'] + '</div>';   //处理失败
                                }
                            }

                            html += '<div class="el-del pull-right mr10 hand" onclick="DeleteFile(\'' + result[i].id + '\')"></div></li>';

                            $("#ulKngDownList").append(html);
                        }

                        //如果是中文状态，下载按钮等样式离右边近一点
                        if ("zh" == "zh") {
                            $(".zhclass").addClass("ml30");
                        }

                        var filesCount = data.paging;
                        downFilesCount = filesCount.count;

                        if (downFilesCount > 99) {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html("99");     //超过99 只显示99
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").show();
                        } else if (downFilesCount == 0) {
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#hidDownCount").hide().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                            $("#hidAddIcon").hide();
                        } else {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").hide();
                        }
                    }
                }
            });
        }

        //记录下载列表里的 知识是否已下载过
        function RecordDownCount(id, url) {
            window.location.href = url;
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/RecordDownCount",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {     //成功则页面有已下载标识
                        //console.log("记录次数");                       
                    } else {
                    }
                }
            });
        }

        //删除下载列表的文件
        function DeleteFile(id) {
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteKngDownFile",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        $("#" + id + "").remove();
                        downFilesCount--;
                        if (downFilesCount > 99) {
                            $("#hidDownCount").html("99");
                            $("#hidAddIcon").show();
                        } else {
                            $("#hidDownCount").html(downFilesCount);
                            $("#hidAddIcon").hide();
                        }

                        if (downFilesCount == 0) {
                            $("#hidDownCount").hide()
                            $("#hidAddIcon").hide();
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");
                        }
                    }
                }
            });
        }

        //清空下载列表
        function DeleteAllFiles() {
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteAllKngDownFiles",
                data: "",
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        downFilesCount = 0;
                        $("#hidDownCount").hide().html(downFilesCount);
                        $("#hidAddIcon").hide();
                        //显示 暂无数据
                        $("#ulKngDownList").hide().empty();
                        $("#divNoMsg").show();
                        $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                    }
                }
            });
        }

        //根据是否开启机构参显示右上角图标
        function showDownLoad() {
            if ($("#hidKngDownType").val() == "1") {
                $("#divDownLoad").removeClass("hidden");
                $("#el-download").show();
            } else {
                $("#el-download").hide();
            }
        }

    </script>
    <script type="text/javascript">

        //下载列表刷新
        var scrollCount = 0;

        /*点击下载按钮下载列表页出现，点击其他区域，关闭列表页面*/
        $(document).ready(function () {
            //页面初始化时隐藏弹窗  
            $('#doc-download').hide();

            //窗口外点击事件  
            $("#el-download").click(function (e) {
                //先重新加载数据再显示
                $("#ulKngDownList").empty();
                scrollCount = 0;
                loadDownDocList(1);

                //阻止事件冒泡,防止波及本按钮  
                e.stopPropagation();
                //显示弹窗  
                $('#doc-download').show();
            });
        });

        $(document).click(function (e) {
            var download = $("#doc-download");
            //判断事件对象不是弹窗对象并且弹窗对象不包含事件对象  
            if (!download.is(e.target) && download.has(e.target).length == 0) {
                //则隐藏弹窗  
                download.hide(100);
            }
        });

        $("#el-download").click(function (e) {
            //阻止事件冒泡，防止波及本按钮  
            e.stopPropagation();
            //隐藏弹窗  
            $('#doc-download').hide(500);
        });

        $("#ulKngDownList").scroll(function () {
            var scrollTop = $(this).scrollTop();
            var divHeight = $(this).height();
            var totalHeight = document.getElementById("ulKngDownList").scrollHeight;
            if (scrollTop + divHeight + 50 > totalHeight) {
                scrollCount++;    //滚动次数累加
                var offs = scrollCount * 9 + 1;
                loadDownDocList(offs);
            }
        });

        //显示默认图
        function showDefaultImg() {
            $("#divDefaultImg").removeClass("hidden");
        }

        //有讲义的音频默认图样式
        function shouwJYImgCss() {
            $("#divDefaultImg").removeClass("jw-center-tu").addClass("jw-center-tu-packge");
            $("#divDefaultImg img").addClass("jw-center-img").removeAttr("height").removeAttr("width");
            showDefaultImg();
        }

     </script>
    
