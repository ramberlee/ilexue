 //发表评论
 function PublishComment() {

 var commentContent = jQuery.trim(jQuery("#txtcomment").val());
 var commentScore = jQuery("#hfGradeLvl").val();
 var flag = 0;
 if ((commentContent == "" || commentContent == jQuery.i18n.map["sty_tip_advance"]) && (commentScore == "" || commentScore == "0")) {
 showWarningMsg(jQuery.i18n.map["sty_msg_entercomment"]);
 return false;
 } else if (commentContent != "" && commentContent != jQuery.i18n.map["sty_tip_advance"] && commentContent.length > 1000) {
 showWarningMsg(jQuery.i18n.map["sty_msg_enternomore1000word"]);
 return false;
 }
 else {
 if (commentContent == jQuery.i18n.map["sty_tip_advance"]) {
 commentContent = "";
 }
 }
 showDealingMsg(jQuery.i18n.map["sys_msg_datasubmission"]);
 var parentId = "";
 var masterId = "c159b820-9494-4a4d-9b9d-233a9d980eb1";
 var commentType = "VideoKnowledge";
 var thirdId = "c159b820-9494-4a4d-9b9d-233a9d980eb1";
 var thirdType = "";
 var packageId = "89fb0321-f192-4981-938d-74e5bbf3fd8e";
 var comType = 0;

 CommentAjax(parentId, masterId, commentType, encodeURIComponent(commentContent), thirdId, thirdType, packageId, commentScore, comType,
		function () {

		});
 }

 //提交评论
 function CommentAjax(parentId, masterId, commentType, commentContent, thirdId, thirdType, packageId, commentScore, comType, successCallback, errorCallback) {
 var checkComment = $("#hidCheckComments").val();
 var methodName = 'PublishCommentNew';
 var arr = '{"parentId":"' + parentId + '","masterId":"' + masterId + '","commentType":"' + commentType + '","commentContent":"' + commentContent + '","thirdId":"' + thirdId + '","thirdType":"' + thirdType + '","packageId":"' + packageId + '","commentScore":"' + commentScore + '","comType":"' + comType + '"}';
 var key = GetValidateKey();
 if (arr && arr.indexOf('{') > -1) {
 arr = arr.replace('{', '{"key":"' + key + '",');
 }
 else {
 arr = arr.replace('{', '{"key":"' + key + '"');
 }
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/PublishCommentNew",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 closeDealingMsg();
 result = eval('(' + result + ')');
 if (result.Status == "Ok") {
 DealAjaxResult(result.Data);

		 if (checkComment == "0") { 
		 showSuccessMsg(jQuery.i18n.map["common_msg_submitsuccess"]);
 } else {
 showSuccessMsg(jQuery.i18n.map["sys_msg_submitcomment"]);
 }

 $("span[el-allow-num]").html(1000);
 jQuery("#txtcomment").val("");
 //评论成功 
 try {
 if ($("#divCommentEmpty").length > 0) {
 $("#divCommentEmpty").hide();
 }
 //发布成功后添加一条虚拟元素刚到第一条
 if (commentContent != "") {
 addFalseElment(result.Data);
 }
 //更新评论列表
 //GetCommentData(1, 10);
 if (Number(commentScore) > 0) {
 var avgStar = $("#hidAvgStar").val();
 var tempCount;
 if (Number(avgStar) > 0) {
 tempCount = Math.round((Number(avgStar) + Number(commentScore)) / 2);
 } else {
 tempCount = Number(commentScore);
 }
 var iconstarlevel = "iconstarlevel" + tempCount;
 $("#avgScoreStar").attr("class", iconstarlevel);
 }
 }
 catch (e) { }

 if (commentScore > 0) {
 //评价后隐藏评分区 
 try {
 jQuery("#divCansetCMStar").hide();
 jQuery("#hfGradeLvl").val("0");
 jQuery("#hidIsShowStar").val("0");
 }
 catch (e) { }
 }

 //回调
 if (successCallback) {
 successCallback();
 }

 } else if (result.Status == "DealERROR") {
 var result = DealAjaxResult(result.Data);
 } else {
 showWarningMsg(result.Data);
 }
 },
 error: function (data) {
 closeDealingMsg();
 showWarningMsg(jQuery.i18n.map["sty_msg_submissionfailed"]);
 }

 });
 }

 //生成GUID类型标示字符串
 function uuid() {
 var s = [];
 var hexDigits = "0123456789abcdef";
 for (var i = 0; i < 36; i++) {
 s[i] = hexDigits.substr(Math.floor(Math.random() * 0x10), 1);
 }
 s[14] = "4"; // bits 12-15 of the time_hi_and_version field to 0010 
 s[19] = hexDigits.substr((s[19] & 0x3) | 0x8, 1); // bits 6-7 of the clock_seq_hi_and_reserved to 01 
 s[8] = s[13] = s[18] = s[23] = "-";

 var uuid = s.join("");
 return uuid;
 }

 //发布成功后添加一条虚拟元素刚到第一条
 function addFalseElment(result) {
 var tempGUID = uuid();
 var isCheckComments = $("#hidCheckComments").val();

 result = JSON.parse(result);
 var str = "";
 str += "<div class='cmitem clearfix'>" +
		"<div class='head_leftimg clearfix'>" +
		"<div class='headimgarea hand'>" +
		"<div class='headborder' onclick=openPersonal('" + result.userId + "')>" +
		"<span title='" + result.userCName + "'><img class='headimage' src='" + result.userPhotoUrl + "' /></span>" +
		"</div>" +
		"</div>" +
		"<div class='itemcontent'>" +
		"<div class='itemheader hand'> " +
		"<div class='headerleft' onclick=openPersonal('" + result.userId + "')>" +
		"<span class='font-bold' title='" + result.userCName + "' >" + result.userCName + "</span>" +
		"</div>" +
		"</div>" +
		"<div class='text-normal break-word'>";
 if (result.isTop == 1) {
 str += "<span class='el-icon-recommend v-mid ml5'></span>";
 } else {
 str += "<span class='box text-color6'>" + result.commentContent + "</span>";
 }

 str += "</div>" +
		"<div class='clearfix mt10'>" +
		"<span class='font-size-12 text-grey pull-left'>" + GetDateLang(result.commentTime, 10, 'zh') + "</span>";
 //if (49b04dcc-4796-437d-ba87-4a8eebf9a22c==result.userId)
 //{
		 str+="<a class='pl15' data-localize='sys_lbl_delete' href='#' onclick='DelComments(\""+result.id+"\",\""+result.userId+"\",\""+result.masterId+"\")'></a>";
 //}
		str+="<div class='pull-right'>";

 if ("0" == isCheckComments) {
 str += "<span class='reply_sp hand font-size-12 text-grey' onclick=showHideDiv('" + tempGUID + "',this) id='" + "reply_" + result.id + "'><span>" + jQuery.i18n.map["sty_lbl_reply"] + "</span></span>";

 if (result.isSupport == 1) {
 str += " <span id='spPraise' class='ml10 el-icon-praise hand' onclick=praiseCount('" + result.id + "')></span>" +
				"<span id='praiseCount' class='ml5 text-link'>" + result.praiseCount + "</span>";
 } else {
 str += "<span id='spPraise" + result.id + "' class='ml10 el-icon-praise-empty hand' onclick=praiseCount('" + result.id + "')></span>" +
				"<span id='praiseCount" + result.id + "' class='ml5 text-disable'>" + result.praiseCount + "</span>";
 }
 }
 else {
 str += "<span class='text-grey pull-right' data-localize='common_lbl_waitingforreview'></span>";
 }

 str += "</div>" +
		"</div>	" +
		"<div class='replypanelHidden mt20' id='divRestore" + tempGUID + "' style='display:none;'>" +
		"<div el-textarea=''>" +
		"<textarea rp='reply' id='txtreply" + tempGUID + "' class='form-control' cols='30' rows='5' maxlength='1000'>" + jQuery.i18n.map["sty_msg_enterreply"] + "</textarea>" +
		"<input class='btn btn-primary btn-smd pull-right mt10'  id='btnReply' style='width:80px;' type='button' onclick=ReplyComment('" + result.id + "','" + tempGUID + "','" + result.masterId + "','" + result.thirdId + "','" + result.thirdType + "','" + result.packageId + "') value=" + jQuery.i18n.map["common_btn_submit"] + " />" +
		"</div>	" +
		"<div class='clearstatic'></div >" +
		"</div>	" +
		"</div>	" +
		"</div>	" +
		"</div>	";

 $(".cmlistcontent").prepend(str);

 loadPropertiesForAsync("dvCommentList");
 }
</script>

<script type="text/javascript">
 var cmClassName = "floatleft cmstarset";
 var cmStarMsg = "";
 //设置评分星星显示
 function setCMTempStar(obj, num, msgid) {
 if (num == '') {
 obj.className = cmClassName;
 document.getElementById(msgid).innerHTML = cmStarMsg;
 } else {
 obj.className = "floatleft cmstarset" + num;
 switch (num) {
 case 1:
 document.getElementById(msgid).innerHTML = " <span class='font-bold'>" + jQuery.i18n.map["sty_lbl_poor"] + "</span> (<span class='cmsnumber'>1</span>" + jQuery.i18n.map["sty_lbl_point"] + ")";
 break;
 case 2:
 document.getElementById(msgid).innerHTML = " <span class='font-bold'>" + jQuery.i18n.map["sty_lbl_evaluationnotgood"] + "</span> (<span class='cmsnumber'>2</span>" + jQuery.i18n.map["sty_lbl_point"] + ")";
 break;
 case 3:
 document.getElementById(msgid).innerHTML = " <span class='font-bold'>" + jQuery.i18n.map["sty_lbl_notbad"] + "</span> (<span class='cmsnumber'>3</span>" + jQuery.i18n.map["sty_lbl_point"] + ")";
 break;
 case 4:
 document.getElementById(msgid).innerHTML = " <span class='font-bold'>" + jQuery.i18n.map["common_lbl_good"] + "</span> (<span class='cmsnumber'>4</span>" + jQuery.i18n.map["sty_lbl_point"] + ")";
 break;
 case 5:
 document.getElementById(msgid).innerHTML = " <span class='font-bold'>" + jQuery.i18n.map["sty_lbl_better"] + "</span> (<span class='cmsnumber'>5</span>" + jQuery.i18n.map["sty_lbl_point"] + ")";
 break;
 default:
 document.getElementById(msgid).innerHTML = "";
 }
 }
 }

 //设置评分值
 function setCMStar(obj, num) {
 cmClassName = obj.parentNode.className;
 document.getElementById('hfGradeLvl').value = num;
 jQuery("#errorScore").hide();
 }

 //个人主页
 function openPersonal(id) {
 openURL("/myhome.htm" + "?userID=" + id);
 }
	
</script>

