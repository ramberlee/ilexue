 /*轮播*/
 $(".banner-box,.banner-boxs").slide({
 titCell: ".hd ul",
 mainCell: ".bd ul",
 effect: "fold",
 interTime: 3500,
 delayTime: 500,
 autoPlay: true,
 autoPage: true,
 trigger: "click"
 });
 /*tab*/
 $(".tab li").click(function (e) {
 var mtarg = $(this);
 if (mtarg.hasClass("active")) return;
 mtarg.addClass("active").siblings().removeClass("active");
 var idx = mtarg.index();
 $(this).parent().next(".tabcon").find(">li:eq('" + idx + "')").show().siblings().hide();
 });
 /*solid banner-loop 滑动 左滑*/
 $(".banner-loop").slide({
 mainCell: ".bd > ul",
 effect: "leftLoop",
 interTime: 3500,
 delayTime: 500,
 vis:5,
 scroll:1,
 autoPlay:true
 });
 /*3D solider*/
 $(function (){
 var newopt ={
 conuntW: "1100",
 countH:"300",
 w1: "482",
 h1: "272",
 w2: "375", //小图宽度
 h2: "205" //小图高度
 };
 i_slide($(".container_image"), newopt);
 });
 /*tab*/
 $(".tabs li").click(function (e) {
 var targ = $(this);
 if (targ.hasClass("active")) return;
 targ.addClass("active").siblings().removeClass("active");
 var idx = targ.index();
 $(this).parents().next(".tabcon").find(">li:eq('" + idx + "')").show().siblings().hide();
 });
</script>

<script type="text/javascript">
 ;
