 getdata("GET", "ucloud--cluster--AAAAAJmBXCJ9fo-Bwjosqobwwbo0XEQWRYzLFQBkbhrX_suGzlWtrOiceMNfKI69CZnrM0CW-uz96Xa4Z2PrKEw78rtyCGq4oZmJBZ-JCSZ9iJitAHRHuIm87RMLhn6LKlcbM5Rqd0t0uDmN8jxSEmqE7zo", arr1, url1, getMyQuePostCount,funcerror);
 function funcerror(r) {
 console.log(r);
 }
 function getdata(typestr,tokenstr,params, url, successhandler,errorhandler) {
 jQuery.ajax({
 type: typestr,
 headers: {
 Accept: "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
 Source: 501,
 Token: tokenstr //"ucloud--cluster--AAAAAJmBXCJ9fo-Bwjosqobwwbo0XEQWRYzLFQBkbhrX_suGzlWtrOiceMNfKI69CZnrM0CW-uz96Xa4Z2PrKEw78rtyCGq4oZmJBZ-JCSZ9iJitAHRHuIm87RMLhn6LKlcbM5Rqd0t0uDmN8jxSEmqE7zo"
 },
 contentType: "application/json",
 url: url,
 xhrFields: {
 withCredentials: true
 },
 dataType: "json",
 data: JSON.stringify(params),
 cache: false,
 success: function (rstr) {
 if (rstr) {
 //console.log(rstr);
 successhandler(rstr);
 }
 },
 error: function (msg) {
 //console.log(msg);
 //return false;
 errorhandler(msg);
 }
 });
 }



