 <script type="text/javascript">
 function showMoreMenu(sender) {
 var l = jQuery(sender).find(".blockmore .c_navmorediv1").length;
 var w = 0;
 if (l == 1) {
 r = 0;
 }

 if (l == 3 || l == 4 || l == 2) {
 r = -270;
 }

 if (l > 4) {
 w = 895;
 r = -220;
 }
 else if (l == 1) {
 r = -146;
 w = l * 220;
 }
 else {
 w = l * 222;
 }

 jQuery(sender).find(".blockmore").css("width", w + "px").css("right", r + "px");
 jQuery(sender).attr('class', 'morehover');
 }
 function hideMoreMenu(sender) {
 jQuery(sender).attr('class', 'more');
 }
 </script>
