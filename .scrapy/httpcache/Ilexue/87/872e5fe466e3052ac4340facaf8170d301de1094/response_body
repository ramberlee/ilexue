 //知识目录
 function getcatalogsList( rstr )
 {
 if ( rstr ) {
 var data = rstr.data;
 var html = "";
 if ( data.length > 0 ) {
 var num=0;
 for ( var i = 0; i < data.length; i++ ) {
 if(num<6&&data[i].catalogRoutingNumber.length==3) {
 html += "<li class='category-item'>";
 html += " <div class='category-caption'>";
 html += " <a href='/kng/knowledgecatalogsearch.htm?id=" + data[i].id + "&rn=" + data[i].catalogRoutingNumber + "&h=menu' target='_blank' title='" + data[i].catalogName + "'>" + data[i].catalogName + "</a>";
 html += " </div>";
 var num1=0;
 for ( var j = 0; j < data.length; j++ ) {
 if(num1<3&&data[j].catalogRoutingNumber.length>3&&data[j].catalogRoutingNumber.length<8&&data[j].catalogRoutingNumber.substr(0,3)==data[i].catalogRoutingNumber) {
 if(num1==0){
 html += " <div class='border-mask'></div>";
 html += "<div class='category-child text-trim'>";
 }
 html += "<a href='/kng/knowledgecatalogsearch.htm?id=" + data[j].id + "&rn=" + data[j].catalogRoutingNumber + "&h=menu' target='_blank' title='" + data[j].catalogName + "'>" + data[j].catalogName + "</a>";
 num1++;
 }
 }
 if(num1>0){
 html += "</div>";
 }
 var num2=0;
 for ( var k = 0; k < data.length; k++ ) {
 if(data[k].catalogRoutingNumber.length>3&&data[k].catalogRoutingNumber.length<8&&data[k].catalogRoutingNumber.substr(0,3)==data[i].catalogRoutingNumber) {
 if(num2==0) {
 html += " <ul class='subcategory'>";
 }
 html += " <li class='subcategory-item'>";
 html += " <p>";
 html += " <a href='/kng/knowledgecatalogsearch.htm?id=" + data[k].id + "&rn=" + data[k].catalogRoutingNumber + "&h=menu' target='_blank' title='" + data[k].catalogName + "'>" + data[k].catalogName + "</a>";
 html += " <span>("+data[k].knowledgeAllCount+")</span>";
 html += " </p>";

 var num3=0;
 for ( var m = 0; m < data.length; m++ ) {
 if(data[m].parentId==data[k].id) {
 if(num3==0) {
 html += " <ul>";
 }
 html += " <li>";
 html += " <a href='/kng/knowledgecatalogsearch.htm?id=" + data[m].id + "&rn=" + data[m].catalogRoutingNumber + "&h=menu' target='_blank' title='" + data[m].catalogName + "'>" + data[m].catalogName + "</a>";
 html += " <span>("+data[m].knowledgeAllCount+")</span>";
 html += " </li>";
 num3++;
 }
 }
 if(num3>0){
 html += " </ul>";
 }




 html += " </li>";
 num2++;
 }
 }
 if(num2>0){
 html += " </ul>";
 }

 html += "</li>";
 num++;
 }
 }
 }
 else {
 html += "<div class=\"emptytextContainer\" style=\"margin-top: 120px;padding-bottom: 100px;\">";
 html += " <div class=\"emptymsg\">敬请期待</div>";
 html += "</div>";
 }
 $("#divCatalogsList").html(html);
 }
 }



 //0.获取导航地址
 function getcustomappfeatures(rstr) {
 if (rstr) {
 //console.log(rstr);
 var num=rstr.data.length;
 for(var i=0;i<num;i++)
 {
 //我的任务
 if(rstr.data[i].id==8)
 $("#mytask").attr("href",rstr.data[i].virtualfullurl);
 //我的考试
 else if(rstr.data[i].id==9)
 $("#mytest").attr("href",rstr.data[i].virtualfullurl);
 //我的培训
 else if(rstr.data[i].id==10)
 $("#mytrain").attr("href",rstr.data[i].virtualfullurl);
 //我的积分
 else if(rstr.data[i].id==21)
 $("#mypoint").attr("href",rstr.data[i].virtualfullurl);
 //我的学分
 else if(rstr.data[i].id==22)
 $("#mylearningpoints").attr("href",rstr.data[i].virtualfullurl);
 //我的问题
 else if(rstr.data[i].id==33)
 $("#linkmyque").attr("href",rstr.data[i].virtualfullurl +"?type=mypost");
 //我的问题
 if(rstr.data[i].id==29)
 $("#bbs").attr("href",rstr.data[i].virtualfullurl);
 //我的收藏
 else if(rstr.data[i].id==20)
 $("#mymyselfknowledge").attr("href",rstr.data[i].virtualfullurl);
 //我的笔记
 else if(rstr.data[i].id==24)
 $("#mynotemgmt").attr("href",rstr.data[i].virtualfullurl);
 }
 }
 }
 //当前登录人的考试个数
 function getexamcount(rstr) {
 if (rstr&&rstr.data) {
 //console.log(rstr);
 $("#ptest").html(rstr.data.normalCount_Starting + rstr.data.normalCount_NotStarted);
 }
 else{
 $("#ptest").html(0);
 }
 }

 //当前登录人的学习计划个数
 function getmystudyplancount(rstr) {
 if (rstr&&rstr.data) {
 //console.log(rstr);
 $("#ptask").html(rstr.data.goingStudyPlanCount + rstr.data.unStartedStudyPlanCount);
 }
 else{
 $("#ptask").html(0);
 }
 }
