<script type="text/javascript">
 //回复评论
 function ReplyComment(id, velocityCount, masterId, thirdId, thirdType, packageId) {
 var txt = $("#txtreply" + velocityCount);
 var comValue = jQuery.trim(txt.val());

 if (comValue == jQuery.i18n.map["sty_msg_enterreply"] || comValue == "") {
 showWarningMsg(jQuery.i18n.map["sty_msg_fillreply"]);
 txt.focus();
 return false;
 }
 if (comValue.length > 1000) {
 showWarningMsg(jQuery.i18n.map["sty_msg_replynomore1000"] + comValue.length + "。");
 return false;
 }
 showDealingMsg(jQuery.i18n.map["sys_msg_datasubmission"]);
 var parentId = id;
 var commentType = "Default";
 var commentScore = 0;
 var commentContent = comValue;
 var comType = 3;

 ReplyAjax(parentId, masterId, commentType, encodeURIComponent(commentContent), thirdId, thirdType, packageId, commentScore, comType, function () {
 window.location.hash = "CommentAnchor";
 });
 }

 //提交回复内容 id ,content
 function ReplyAjax(parentId, masterId, commentType, commentContent, thirdId, thirdType, packageId, commentScore, comType) {
 var checkComment = $("#hidCheckComments").val();

 var arr = '{"parentId":"' + parentId + '","masterId":"' + masterId + '","commentType":"' + commentType + '","commentContent":"' + commentContent + '","thirdId":"' + thirdId + '","thirdType":"' + thirdType + '","packageId":"' + packageId + '","commentScore":"' + commentScore + '","comType":"' + comType + '"}';
 var key = GetValidateKey();
 if (arr && arr.indexOf('{') > -1) {
 arr = arr.replace('{', '{"key":"' + key + '",');
 }
 else {
 arr = arr.replace('{', '{"key":"' + key + '"');
 }
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/PublishCommentNew",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 closeDealingMsg();
 result = eval('(' + result + ')');
 if (result.Status == "Ok") {
 DealAjaxResult(result.Data);
 if (checkComment == "0") {
 showSuccessMsg(jQuery.i18n.map["common_msg_submitsuccess"]);
 } else {
 showSuccessMsg(jQuery.i18n.map["sys_msg_submitcomment"]);
 }
 //
 GetCommentData("0", "10");
 } else if (result.Status == "DealERROR") {
 var result = DealAjaxResult(result.Data);
 } else {
 showWarningMsg(result.Data);
 }
 },
 error: function (data) {
 closeDealingMsg();
 showWarningMsg(jQuery.i18n.map["sty_msg_submissionfailed"]);
 }
 });
 }


 //删除评论 评论Id和评论者的Id
 function DelComments(id, userId, masterId) {
 var tip = jQuery.i18n.map["sys_msg_submitdelcomment"];

 if (!confirm(tip)) {
 return false;
 }

 var arr = '{"id":"' + id + '","userId":"' + userId + '","masterId":"' + masterId + '"}';

 var key = GetValidateKey();
 if (arr && arr.indexOf('{') > -1) {
 arr = arr.replace('{', '{"key":"' + key + '",');
 }
 else {
 arr = arr.replace('{', '{"key":"' + key + '"');
 }

 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/DeleteComment",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 closeDealingMsg();
 result = eval('(' + result + ')');
 if (result.Status == "OK") {

 GetCommentData("0", "10");

 showSuccessMsg(jQuery.i18n.map["sys_msg_delcomment"]);

 } else if (result.Status == "DealERROR") {
 var result = DealAjaxResult(result.Data);
 } else if (result.Status == "ForBid") {
 showWarningMsg(jQuery.i18n.map["sys_msg_forbiddelcomment"]);
 } else {
 showWarningMsg(result.Data);
 }
 },
 error: function (data) {
 closeDealingMsg();
 showWarningMsg(jQuery.i18n.map["sys_msg_delcommentfail"]);
 }
 });
 }

 //点赞
 function praiseCount(id) {
 var arr = '{"id":"' + id + '"}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/PraiseComment",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (msgResult) {
 msgResult = eval('(' + msgResult + ')');
 if (msgResult.Status == "OK") {
 var tempCount = 0;
 var count = $("#praiseCount" + id).html();
 if (count == "" || typeof (count) == "undefined") {
 $("#praiseCount" + id).html(1);
 } else {
 tempCount = parseInt(count) + 1;
 $("#praiseCount" + id).html(tempCount);
 }
 $("#praiseCount" + id).removeClass("ml5 text-disable").addClass("ml5 text-link");
 $("#spPraise" + id).removeClass("el-icon-praise-empty hand ml10").addClass("el-icon-praise hand ml10");
 }
 else {
 showWarningMsg(msgResult.Data);
 }
 }
 });
 }

 //获取评论数据
 function GetCommentData(pageIndex, pageSize) {
 var key = "MasterID";
 var value = "eeafa0f3-7449-4af3-83e2-8ddca05da389";

 if (pageIndex && pageIndex != "") {
 key += "|PageIndex";
 value += "|" + pageIndex;
 }

 if (pageSize && pageSize != "") {
 key += "|PageSize";
 value += "|" + pageSize;
 }

 var PackageID = "eeafa0f3-7449-4af3-83e2-8ddca05da389";
 if (PackageID && PackageID != "") {
 key += "|PackageID";
 value += "|" + PackageID;
 }



 GetDataByLabelENameWithKng("dvCommentList", "KngCommentList30", 0, 0, key, value);
 }

 //显示回复框
 function showHideDiv(id, obj) {
 $("span[el-allow-num]").html(1000);
 $("textarea[rp='reply']").val("");
 try {
 jQuery("#txtreply" + id).data('textarea').resetArea();
 } catch (e) { }
 if ($(obj).text() == jQuery.i18n.map["sty_lbl_canclereply"]) {
 $(obj).text(jQuery.i18n.map["sty_lbl_reply"]);
 jQuery("#divRestore" + id).slideToggle("slow");
 return;
 }

 $(".reply_sp").each(function () {
 if ($(this).text() == jQuery.i18n.map["sty_lbl_canclereply"]) {
 $(this).text(jQuery.i18n.map["sty_lbl_reply"]);
 }
 });

 jQuery(".replypanelHidden").each(function () {
 if ($(this).attr("id") != "divRestore" + id) {
 $(this).slideUp("slow");
 }
 });

 if ($(obj).text() == jQuery.i18n.map["sty_lbl_reply"]) {
 $(obj).text(jQuery.i18n.map["sty_lbl_canclereply"]);
 jQuery("#divRestore" + id).slideToggle("slow");
 }

 }

 //页面加载
 jQuery(document).ready(function () {
 //分页代码注册
 var prev = jQuery("#navComment a[t='PreName']");
 var next = jQuery("#navComment a[t='NextName']");
 if (prev.length > 0) {
 var url = prev.attr("href");
 var index = url.substring(url.indexOf("pi=") + 3);
 prev.attr("href", "javascript:void(0);");
 prev.click(function () { GetCommentData(index, "10"); });
 }
 if (next.length > 0) {
 var url2 = next.attr("href");
 var index2 = url2.substring(url2.indexOf("pi=") + 3);
 next.attr("href", "javascript:void(0);");
 next.click(function () { GetCommentData(index2, "10"); });
 }
 jQuery("#navComment a.pagenumber").each(function () {
 var url3 = jQuery(this).attr("href");
 var index3 = url3.substring(url3.indexOf("pi=") + 3);
 jQuery(this).attr("href", "javascript:void(0);");
 jQuery(this).click(function () { GetCommentData(index3, "10"); });
 });
 //页面大小代码注册
 jQuery("#navComment div.listitem").each(function () {
 var code = jQuery(this).attr("id").substring(jQuery(this).attr("id").indexOf("_") + 1);
 var size = jQuery(this).text();
 this.onclick = function () { GetCommentData("1", size); };
 });
 $("#spCount").text("5");
 });

