<script type="text/javascript">
 //兑换
 function knowlegeExhange(){
	var knowledgeId=$("#hidKnowledgeID").val(); 		
	var arr='{"knowledgeId":"'+knowledgeId+'"}';
	jQuery.ajax({
		 type: "POST",
		 contentType: "text/json",
		 url: "/kng/services/KngComService.svc/KnowledgeExchange",
		 data: arr,
		 dataType: 'json',
		 cache: false,
		 success: function (result) {		 
		 result = eval('(' + result + ')');
		 if (result.Status == "Ok") {		 
					$("#divKnowledgeDetail").hide();
					$("#divExchangeSuccess").attr("style","width:1100px;height:370px;margin-top: -185px;");	
					var courseOrder=eval("(" + result.Data + ")")
					$("#orderNum").html(courseOrder.orderId);					
		 } else if(result.Status == "ERROR"){					
		 $("#divKnowledgeDetail").hide();
					$("#divExchangeFail").attr("style","width:1100px;height:370px;margin-top: -185px;");	
					showWarningMsg(result.Data);					
		 }
		 },
		 error: function (data) {
		 
		 }
		});	
 }
 
 
 
 //重试一次
 function resetAgain(){
	$("#divExchangeFail").hide();
	$("#divKnowledgeDetail").show();	
 }
 
 
 //关闭
 function closeDiv(i) {
 	$("#divExchange").hide();
	if(i){
		//window.location=window.location;
		window.location.reload();
	}	
 } 
 
 function closeFailDiv(){
	$("#divExchangeFail").hide();
	$("#divExchange").hide();
 }
 	
</script>

