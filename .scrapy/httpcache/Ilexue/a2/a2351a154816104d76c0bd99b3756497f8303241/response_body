    <div id="divValidateKey" style="display: none;">d078ee9b-e3b1-46cb-86cd-41603bdd4977</div>
    <script>
        $("#list dd").hover(function () {
            $(this).addClass("active").siblings().removeClass("active");
        }, function () {
            $(this).removeClass("active");
            $("#list dd").eq(0).addClass("active");
        });

        $(".el-knghome-tree-block > li").hover(function () {
            var obj = $(this).find(".el-knghome-panel"),
            h = obj.outerHeight(),
            offset = $(".el-knghome-left").offset(),
            poff = $(this).offset();
            if (poff.top - offset.top + h > 470) {
                obj.css("top", 470 - poff.top + offset.top - h);
            }
            $(this).addClass("active");
        }, function () {
            $(this).removeClass("active");
        });

        $(".el-knghome-pancel-close").on("click", function () {
            $(this).parents("li").removeClass("active");
        });
    </script>
