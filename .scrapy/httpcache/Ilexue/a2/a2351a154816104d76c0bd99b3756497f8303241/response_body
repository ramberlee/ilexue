<script type="text/javascript">
 $(function () {
 //获取相关知识的数量
 var methodName = 'GetApprovedKnowledgeCount';
 var arr = {};
 AjaxAsyncCallMethodWithKng(methodName, arr, function (result) {
 loadKnowledge(result);
 }, false, true, true);

 
 $("#spanShareKnowledgeCount").text(0)

 });

 //绑定知识数量
 function loadKnowledge(obj) {
 if (obj != null) {
 var intObj = obj.split('|');
 var kngCount = intObj[4].length;

 var orderParam = getCookie("ELEARNING_00025");
 var urlParam = "";
 if (orderParam != "") {
 var pa = orderParam.split('|');
 urlParam = "&sf=" + decodeURI(pa[0]) + "&s=" + pa[1] + "&st=" + pa[2];
 }

 //其他数量
 var otherCount = intObj[4] - intObj[0] - intObj[1] - intObj[2];
 var videoHtml = '<a href="/kng/knowledgecatalogsearch.htm?t=VideoKnowledge&h=menu' + urlParam + '" target="_blank" class="text-grey font-num">' + intObj[0] + '</a>';
 var documentHtml = '<a href="/kng/knowledgecatalogsearch.htm?t=DocumentKnowledge&h=menu' + urlParam + '" target="_blank" class="text-grey font-num">' + intObj[1] + '</a>';
 var courseHtml = '<a href="/kng/knowledgecatalogsearch.htm?t=CourseKnowledge&h=menu' + urlParam + '" target="_blank" class="text-grey font-num">' + intObj[2] + '</a>';
 var otherHtml = '<a href="/kng/knowledgecatalogsearch.htm?h=menu' + urlParam + '" target="_blank" class="text-grey font-num">' + otherCount + '</a>';
 jQuery('#knowledgeVideoCount').html(videoHtml);
 jQuery("#knowledgeDocument").html(documentHtml);
 jQuery("#knowledgeCourseCount").html(courseHtml);
 jQuery("#knowledgeOtherCount").html(otherHtml);

 var count = intObj[4], temp = [];
 for (var i = count.length - 1, j = 0; i >= 0; i--, j++) {
 if (j == 3) {
 temp.unshift(" <span class='el-knghome-number v-mid'>" + count.charAt(i) + "</span> <span class='el-knghome-comma v-bottom'>,</span>");
 } else {
 temp.unshift(" <span class='el-knghome-number v-mid'>" + count.charAt(i) + "</span>");
 }
 }
 temp = temp.join("");
 jQuery("#knowledgeTotalCount").html(temp);
 }
 }

 //刷新
 function refresh() {
 window.location.href = window.location.href;
 }

 function showUploadDialog(type, action) {
var id="";
 if (type == "file") {
 if (action == "Edit") {
 ShowIFrameDialog(jQuery.i18n.map['kng_lbl_uploadfile'], 964, 550, "/kng/Upload/ShareEdit.aspx?id=" + id + "&OP=" + action, abjuration);
 }
 else {
 ShowIFrameDialog(jQuery.i18n.map['kng_lbl_uploadfile'], 964, 720, '/kng/Upload/Share.aspx?OP=' + action, abjuration);
 }
 }
 else if (type == "knowledgepackage") {
 if (action == "Edit") {
 ShowIFrameDialog(jQuery.i18n.map['kng_lbl_uploadfile'], 980, 750, '/kng/Cou/CourseProfileDetailForUser.aspx?ID=' + id + '&OP=' + action + '&IsForList=1&IsCoursePackage=1', abjuration);
 }
 else {
 ShowIFrameDialog(jQuery.i18n.map['kng_lbl_uploadfile'], 980, 750, '/kng/Cou/CourseProfileDetailForUser.aspx?OP=' + action + '&IsForList=1&IsCoursePackage=1', abjuration);
 }
 }
 else if (type == "xuanyes") {
 if (action == "Edit") {
 ShowIFrameDialog(jQuery.i18n.map['kng_tit_editxuanyes'], 980, 630, "/kng/Upload/ShareXuanYes.aspx?id=" + id + "&OP=" + action);
 }
 else {
 ShowIFrameDialog(jQuery.i18n.map['kng_lbl_publishshinypage'], 980, 630, '/kng/Upload/ShareXuanYes.aspx?OP=' + action);
 }
 }
 else if (type == "NewUpload") {
 var url = '/kng/upload/uploadkng.htm';
 window.open(url);
 }
 else if (type == "LocalNewUpload") {
 var url = '/apps/kng/upload/uploadkngmix.htm';
 window.open(url);
 }
 else if (type == "microClass")
 {
 var url = '/apps/kng/workcreate.htm';
 url+='?from=0';
 window.open(url);
 }
 }

 function showKnowledgeType(type) {
 var orderParam = getCookie("ELEARNING_00025");
 var urlParam = "";
 if (orderParam != "") {
 var pa = orderParam.split('|');
 urlParam = "&sf=" + decodeURI(pa[0]) + "&s=" + pa[1] + "&st=" + pa[2];
 }
 if (type == "") {
 var url = "/kng/knowledgecatalogsearch.htm?h=menu" + urlParam; //知识目录查看url
 } else {
 var url = "/kng/knowledgecatalogsearch.htm?t=" + type + "&h=menu" + urlParam; //知识目录查看url
 }
 
 window.open(url);
 }
</script>

