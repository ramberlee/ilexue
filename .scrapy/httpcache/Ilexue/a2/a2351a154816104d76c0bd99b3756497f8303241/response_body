<script type="text/javascript">
 $(document).ready(function () {
 loadPropertiesForAsync("divKngIndexCatalogInfo");
 });

 $("#divTree").mCustomScrollbar({
 
 });

 function toggleCatalog(e, r) {
 var behaviour = jQuery(e).parent().attr("behaviour"); //div
 var divNode = jQuery(e).parent();
 var cname = jQuery(e).attr("class");
 if (cname == "tplus") {
 jQuery(e).attr("class", "tminus");
 }
 else if (cname == "tminus") {
 jQuery(e).attr("class", "tplus");
 }
 else if (cname == "lminus") {
 jQuery(e).attr("class", "lplus");
 }
 else if (cname == "lplus") {
 jQuery(e).attr("class", "lminus");
 }
 else
 { }
 if (behaviour == "Expand") {
 if (r == "0") {

 divNode.children(":first").attr("class", "minus");
 divNode.next().css("display", "");
 var second = divNode.next();
 second.find("div :hidden").show();
 second.find(".tplus").attr("class", "tminus");
 second.find(".lplus").attr("class", "lminus");
 divNode.attr("behaviour", "Collaspe");

 }
 else {
 if (divNode.children("div").css("display") == "none") {
 divNode.children("div").css("display", "");
 divNode.attr("behaviour", "Expand");
 jQuery(e).next(".folder").attr("class", "folder1");
 }
 else {
 divNode.children("div").css("display", "none");
 divNode.attr("behaviour", "Collaspe");
 jQuery(e).next(".folder1").attr("class", "folder");
 }
 }
 }
 else {

 if (r == "0") {

 divNode.children(":first").attr("class", "plus");
 divNode.next().css("display", "none");
 divNode.attr("behaviour", "Expand");

 }
 else {
 if (divNode.children("div").css("display") == "none") {
 divNode.children("div").css("display", "");
 divNode.attr("behaviour", "Expand");
 jQuery(e).next(".folder").attr("class", "folder1");
 }
 else {
 divNode.children("div").css("display", "none");
 divNode.attr("behaviour", "Collaspe");
 jQuery(e).next(".folder1").attr("class", "folder");
 }
 }
 }
 }

 function setCookie(cname, cvalue, exdays) {
 var d = new Date();
 d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
 var expires = "expires=" + d.toUTCString();
 document.cookie = cname + "=" + cvalue + "; " + expires;
 }
 var isSc = 0;
 function changeTreeOrMode() {
 var title = jQuery("#divTreeOrMode").html();
 var displayStyle = "";
 if (title == jQuery.i18n.map['kng_lbl_menudisplay']) {
 jQuery("#divTreeOrMode").html(jQuery.i18n.map['kng_lbl_showastree']);
 jQuery('#divMenu').show();
 jQuery('#divTree').hide();

 displayStyle = "menu";
 }
 else {
 jQuery("#divTreeOrMode").html(jQuery.i18n.map['kng_lbl_menudisplay']);
 jQuery('#divMenu').hide();
 jQuery('#divTree').show();

 displayStyle = "tree";
 }
 setCookie("ELEARNING_00010", displayStyle, 30);
 if (isSc==0) {
 $("#divTree").mCustomScrollbar({
 });
 isSc = 1;
 }
 }
</script>
 
