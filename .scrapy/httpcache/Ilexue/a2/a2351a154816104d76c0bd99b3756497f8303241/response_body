<script type="text/javascript" src="https://staticbd.yunxuetang.cn/yxt/SystemFiles//Js/new/plugin/jquery.SuperSlide.2.1.1.js?v=111"></script>
<script type="text/javascript">
 //呈现数据
 function displayData(rn, op, searchType) {

 var url = "/kng/knowledgecatalogsearch.htm";
 var paramString = "";
 if (url.indexOf("?") > -1) {
 url = url.split("?")[0];
 }
 else {
 url = url;
 }

 var orderParam = getCookie("ELEARNING_00025");
 var urlParam = "";
 if (orderParam != "") {
 var pa = orderParam.split('|');
 urlParam = "&sf=" + decodeURI(pa[0]) + "&s=" + pa[1]+ "&st=" + pa[2];
 }

 var ind = jQuery(op).attr("id");
 if (searchType) {
 paramString = "?id=" + ind + "&rn=" + rn + "&h=" + searchType + urlParam;
 }
 else {
 paramString = "?id=" + ind + "&rn=" + rn + urlParam;
 }

 url = url + paramString+"&mode=1";
 window.open(url);
 }

 //查看更多
 function displayMore(sf, s) {
 var url = "/kng/knowledgecatalogsearch.htm";
 var urlParam = "?sf=" + sf + "&s=" + s;
 var orderParam = getCookie("ELEARNING_00025");
 if (orderParam != "") {
 var pa = orderParam.split('|');
 urlParam += "&st=" + pa[2];
 }

 url = url + urlParam;
 window.open(url);
 }

 function ShowTaIndex(id) {
 openURL("/myhome.htm" + "?userID=" + id + "&stype=share");
 }
</script></div>
