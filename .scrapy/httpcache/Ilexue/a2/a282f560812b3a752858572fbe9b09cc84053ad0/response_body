        /*从未完成到已完成*/
        var iscurfinish = undefined;

        //检查是否收藏
        function CheckAddToFavorite(orgID, userID, knowledgeID, packageID, masterID, masterType, elementClientID, isInnerHtml) {
            if ($('#hfIsFavorite').val() == "1") {
                var arr = '{"knowledgeID":"' + knowledgeID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/DeleteFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")")
                        if (result.Status == "OK") {
                            $("#liFavorite").prop("class", "");
                            $("#hfIsFavorite").val("0");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_cancelcollectionsuccess']);
                        }
                        else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            } else {
                var arr = '{"knowledgeID":"' + knowledgeID + '","packageID":"' + packageID + '","masterType":"' + masterType + '","masterID":"' + masterID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/AddFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")")
                        if (result.Status == "OK") {
                            $("#liFavorite").prop("class", "done");
                            $("#hfIsFavorite").val("1");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_collectionsuccess']);
                        }
                        else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            }
        }

        //赞某对象
        function supportKng(masterIDs, type) {
            checkLogin2(function () {
                var arr = '{"masterIDs":"' + masterIDs + '","type":' + type + ',"remark":"知识学习时的赞"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/SupportObject",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        if (result == "1") {
                            $("#liSupport").removeClass().addClass("done pr");
                            $("#hfIsSupport").val("1");
                            var num = $("#praiseNum").html();
                            if (parseInt(num) < 99) {
                                $("#praiseNum").html(parseInt(num) + 1);
                                $("#praiseNum").removeClass().addClass("el-praise-num");
                            } else if (parseInt(num) == 99) {
                                $("#praiseNum").html("99+");
                            }
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_successpraise']);
                        } else if (result == "-888") {
                            showWarningMsg(jQuery.i18n.map['kng_lbl_havepraised']);
                        }
                    }
                });
            });
        }

        //返回
        function GoBack() {
            var url = $('#hfHomeURL').val();
            var m = getQueryString("m");
            if (m == 1) {
                if (url.indexOf("?") == -1) {
                    url = url + "?m=1";
                }
                else {
                    url = url + "&m=1";
                }
            }
            location.href = url;
        }

        //下载列表的文件数量
        var downFilesCount = 0;

        //下载文件  
        function DownloadDocument() {
            var key = $("#hidDownKey").val();

            //有机构参，加水印下载，否则走之前逻辑
            var fileIdValue = $("#hidFileID").val();
            if ($("#hidKngDownType").val() == "1" && fileIdValue != "") {
                var knowledgeId = $("#hidKnowledgeID").val();
                var packageID = "2abd9bbd-cfa0-4f14-8760-1d210d7ef507";

                var arr = '{"fileID":"' + knowledgeId + '","masterId":"' + packageID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/KngDownLoadDoc",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (data) {
                        if (data) {
                            //成功之后再加载列表，看看是否有下载记录
                            showSuccessMsg(jQuery.i18n.map['msg_lbl_knowledgedocdowm']);
                            jQuery.ajax({
                                type: "POST",
                                contentType: "text/json",
                                url: "/kng/services/KngComService.svc/GetKngDownDocList",
                                data: '{"limit":"10000","offset":"1"}',
                                dataType: 'json',
                                cache: false,
                                success: function (data2) {
                                    if (data2 != "") {
                                        var result = data2.paging;
                                        downFilesCount = result.count;
                                        if (downFilesCount > 99) {
                                            $("#hidDownCount").show().addClass("count-ani").html("99");
                                            $("#hidAddIcon").show();
                                        } else {
                                            $("#hidDownCount").show().addClass("count-ani").html(downFilesCount);
                                            $("#hidAddIcon").hide();
                                        }
                                        setTimeout(removeCountani, 1000);
                                    }
                                }
                            });
                        }
                    }
                });

            } else {
                //下载(之前逻辑)
                DownloadAttachmentWithKng(key);
            }

            return false;
        }

        //移除添加下载的动效
        function removeCountani() {
            $("#hidDownCount").removeClass("count-ani");
        }

        //下载管理列表
        function loadDownDocList(offset) {
            var arr = '{"limit":"9","offset":"' + offset + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngDownDocList",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data != "") {
                        var result = data.datas;

                        for (var i = 0; i < result.length; i++) {

                            var html = '<li id=' + result[i].id + ' class="doc-line-list clearfix">';

                            if (result[i].fileType == "PDF") {
                                html += '<span class="icon-Knowledge-pdf pull-left">';
                            } else if (result[i].fileType == "Image") {
                                html += '<span class="icon-Knowledge-image pull-left">';
                            }

                            html += '</span><div class="list-body pull-left ml10 mr20"><div class="ellipsis text-normal hand font-size-13" title="' + result[i].knowledgeName + '">' + result[i].knowledgeName + '</div><div class="font-size-13 text-grey clearfix el-list-date">' + result[i].updateDate + '</div></div>';

                            if (result[i].isDownload == "1") {    //是否已下载 0：未下载 1：已下载 
                                html += '<div class="pull-left font-size-13 clearfix el-download-status zhclass"><div class="el-downloading"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div><div class="text-grey el-have-downloaded">(' + jQuery.i18n.map['lbl_msg_download'] + ')</div></div>';    //已下载
                            } else {
                                if (result[i].status == "0") {    //状态 0：处理中 1：处理完成 2:处理失败
                                    html += '<div class="text-grey font-size-13 pull-left clearfix being-processed zhclass">' + jQuery.i18n.map['lbl_msg_processing'] + '</div>';    //处理中
                                } else if (result[i].status == "1") {
                                    html += '<div class="text-grey font-size-13 pull-left el-downloading clearfix zhclass"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div>';
                                } else {
                                    html += '<div class="font-size-13 pull-left clearfix text-red zhclass">' + jQuery.i18n.map['lbl_msg_processfailed'] + '</div>';   //处理失败
                                }
                            }

                            html += '<div class="el-del pull-right mr10 hand" onclick="DeleteFile(\'' + result[i].id + '\')"></div></li>';

                            $("#ulKngDownList").append(html);
                        }

                        //如果是中文状态，下载按钮等样式离右边近一点
                        if ("zh" == "zh") {
                            $(".zhclass").addClass("ml30");
                        }

                        var filesCount = data.paging;
                        downFilesCount = filesCount.count;

                        if (downFilesCount > 99) {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html("99");     //超过99 只显示99
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").show();
                        } else if (downFilesCount == 0) {
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#hidDownCount").hide().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                            $("#hidAddIcon").hide();
                        } else {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").hide();
                        }
                    }
                }
            });
        }

        //记录下载列表里的 知识是否已下载过
        function RecordDownCount(id, url) {
            window.location.href = url;
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/RecordDownCount",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {     //成功则页面有已下载标识
                        //console.log("记录次数");                       
                    } else {
                    }
                }
            });
        }

        //删除下载列表的文件
        function DeleteFile(id) {
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteKngDownFile",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        $("#" + id + "").remove();
                        downFilesCount--;
                        if (downFilesCount > 99) {
                            $("#hidDownCount").html("99");
                            $("#hidAddIcon").show();
                        } else {
                            $("#hidDownCount").html(downFilesCount);
                            $("#hidAddIcon").hide();
                        }

                        if (downFilesCount == 0) {
                            $("#hidDownCount").hide()
                            $("#hidAddIcon").hide();
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");
                        }
                    }
                }
            });
        }

        //清空下载列表
        function DeleteAllFiles() {
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteAllKngDownFiles",
                data: "",
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        downFilesCount = 0;
                        $("#hidDownCount").hide().html(downFilesCount);
                        $("#hidAddIcon").hide();
                        //显示 暂无数据
                        $("#ulKngDownList").hide().empty();
                        $("#divNoMsg").show();
                        $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                    }
                }
            });
        }

        //根据是否开启机构参显示右上角图标
        function showDownLoad() {
            if ($("#hidKngDownType").val() == "1") {
                $("#divDownLoad").removeClass("hidden");
                $("#el-download").show();
            } else {
                $("#el-download").hide();
            }
        }

        ///立即开始当前的知识学习
        function StartCurStudy() {
            if (confirm(jQuery.i18n.map['kng_lbl_studytrackcomfirm'])) {
                var id = $("#hidKnowledgeID").val();
                var arr = '{"knowledgeID":"' + id + '","packageID":"2abd9bbd-cfa0-4f14-8760-1d210d7ef507","masterID":"","masterType":""}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/ChangeStudyKnowledge",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        var url = window.location.href;
                        if (url.indexOf("tmp=") < 0) {
                            if (url.indexOf("?") > -1) {
                                window.location.search += "&tmp=" + Date.parse(new Date());
                            } else {
                                window.location.search += "?tmp=" + Date.parse(new Date());
                            }
                        }
                        else {
                            var c = url.indexOf("tmp=");
                            var v = url.substring(c + 4, c + 17);
                            var v1 = v + 1;
                            url = url.replace(v, v1);
                            window.location.href = url;
                        }
                    }
                });
            }
        }

        //隐藏课程的提醒后记录当前隐藏的时间
        function HideCourseWarm(obj) {
            var kid = $("#hfUserKngID").val();
            var arr = '{"userKngID":"' + kid + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/AddExpiredTimeByUserKngID",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function () {
                }
            });
            $(obj).parent('.tipbox').hide();
        }

        //0：评论  1:问答    2：笔记
        function changeContentTab(obj, index) {
            if ($(obj).hasClass("active")) {

            } else {
                if (index == 0) {
                    $("#liComment").addClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").removeClass("active");
                    $("#divComment").show();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").hide();
                    var isShowStar = $("#hidIsShowStar").val();
                    var userPhoto = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divComment', 'KnowledgeCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', 'dd5da5b0-2517-4756-9d18-8f7f21bfed4c|%e7%94%a8%e6%bc%94%e7%a4%ba%e8%af%b4%e8%af%9d.%e9%ba%a6%e8%82%af%e9%94%a1%e5%95%86%e5%8a%a1%e6%b2%9f%e9%80%9a%e5%ae%8c%e5%85%a8%e6%89%8b%e5%86%8c|DocumentKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|cae89804-ea78-4402-a291-ada1a551de96|刘林平|dd5da5b0-2517-4756-9d18-8f7f21bfed4c|2abd9bbd-cfa0-4f14-8760-1d210d7ef507|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
                    var key = "MasterID|SourceID|PackageID|UserID|PageIndex|PageSize";
                    var value = "dd5da5b0-2517-4756-9d18-8f7f21bfed4c|dd5da5b0-2517-4756-9d18-8f7f21bfed4c|2abd9bbd-cfa0-4f14-8760-1d210d7ef507|49b04dcc-4796-437d-ba87-4a8eebf9a22c|0|10";
                    GetDataByLabelENameWithKng("dvCommentList", "KngCommentList30", 0, 0, key, value);

                } else if (index == 1) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").addClass("active");
                    $("#liNote").removeClass("active"); 
                    $("#divComment").hide();
                    $("#divAskQuestion").show();
                    $("#divKngNote").hide();
                    var title = $("#hidTitle").val();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divAskQuestion', 'KngViewQuestionInfo', 'imageUrl', hfimageurl);
                    GetDataByLabelENameWithKng("divQuestionList", "KngQuestionList", 0, 0, "title", encodeURIComponent(title));

                } else if (index == 2) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").addClass("active");
                    $("#divComment").hide();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").show();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divKngNote', 'KngViewNoteInfo', 'KnowledgeID|KnowledgeType|ImageUrl', 'dd5da5b0-2517-4756-9d18-8f7f21bfed4c|DocumentKnowledge|' + hfimageurl);
                    loadPropertiesForAsync("divKngNote");
                }

            }
        }

        //锚点移动
        function hashAction(id) {
            var t = $("#" + id).offset().top;
            $(window).scrollTop(t);
            $("#" + id).click();
        }

    </script>
 
