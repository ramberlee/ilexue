 //设置播放器完成事件监听函数。
 player.onComplete = function (event) {
 
 if(!firstComplete)
 {

 jQuery("#hidViewSchedule").val("0");
 delCookie(CookieName);

var sTime=standardStudyHours-actualStudyHours;

 if(sTime>120) {
	//StopTimer();
	setTimeout(StopTimer, 1000);
 }

 if("1"=="0")
 {
 return;
 }

 if(""=="1")
 {
 
 firstComplete=true;
 window.open("/kng/course/package/video/c8e448ca6a1f4b289392bc00f234c32d_8251363685dc4b9288fb6975763e91f3.html?1=1",'_self')

 }else{

 var schedule=$("#ScheduleText").text();
 var vediolength=parseInt(player.bdPlayer.getDuration());
 if(vediolength<15 && schedule.indexOf('100')==-1)
 {
 completedLaveTimeStudy();
 }

 
 //如果倒计时小于60s（备注：断点续播时，视频播放时间小于120秒，导致播放完成不能提交进度）
 try {
 var laveTime=standardStudyHours-actualStudyHours;
 if(laveTime<120 && schedule.indexOf('100')==-1)
 {
 completedLaveTimeStudy();
 }
 } catch(ex) {
 console.log("进度100%");
 }

 if("False".toLowerCase()=="true")
 {
 //控制学习顺序，到100%时自动续播
 if(schedule.indexOf('100')>-1)
 {
 firstComplete=true;
 window.open("/kng/course/package/video/c8e448ca6a1f4b289392bc00f234c32d_8251363685dc4b9288fb6975763e91f3.html?1=1",'_self');
 }
 }else{
 firstComplete=true;
