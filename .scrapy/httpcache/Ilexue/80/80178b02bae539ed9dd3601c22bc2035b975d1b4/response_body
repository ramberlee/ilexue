        function changeContentTab(obj, index) {
            if ($(obj).hasClass("active")) {

            } else {
                if (index == 0) {
                    $("#liComment").addClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").removeClass("active");
                    $("#divComment").show();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").hide();
                    var isShowStar = $("#hidIsShowStar").val();
                    var userPhoto = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', '89f4b4c9-9807-4a65-83ba-ae6550234911|%e5%bf%ab%e9%80%9f%e9%80%89%e6%8b%a9%e7%9a%84%e6%8a%80%e5%b7%a7|VideoKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|2408ce95-4235-440f-b1f4-ee5958cf36ea|管理员|89f4b4c9-9807-4a65-83ba-ae6550234911|d3e979fd-f2ed-441b-be4a-f6847dd4dd43|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
                    var key = "MasterID|SourceID|PackageID|UserID|PageIndex|PageSize";
                    var value = "89f4b4c9-9807-4a65-83ba-ae6550234911|89f4b4c9-9807-4a65-83ba-ae6550234911|d3e979fd-f2ed-441b-be4a-f6847dd4dd43|49b04dcc-4796-437d-ba87-4a8eebf9a22c|0|10";
                    GetDataByLabelENameWithKng("dvCommentList", "KngCommentList30", 0, 0, key, value);

                } else if (index == 1) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").addClass("active");
                    $("#liNote").removeClass("active");
                    $("#divComment").hide();
                    $("#divAskQuestion").show();
                    $("#divKngNote").hide();
                    var title = $("#hidTitle").val();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divAskQuestion', 'KngViewQuestionInfo', 'imageUrl', hfimageurl);
                    GetDataByLabelENameWithKng("divQuestionList", "KngQuestionList", 0, 0, "title", title);

                } else if (index == 2) {
                    $("#liComment").removeClass("active");
                    $("#liQuestion").removeClass("active");
                    $("#liNote").addClass("active");
                    $("#divComment").hide();
                    $("#divAskQuestion").hide();
                    $("#divKngNote").show();
                    var hfimageurl = $("#hidUserPhoto").val();
                    SyncLoadHtmlContentWithKng('divKngNote', 'KngViewNoteInfo', 'KnowledgeID|KnowledgeType|ImageUrl', '89f4b4c9-9807-4a65-83ba-ae6550234911|VideoKnowledge|' + hfimageurl);
                    loadPropertiesForAsync("divKngNote");
                }
            }
        }

        //锚点移动
        function hashAction(id) {
            var t = $("#" + id).offset().top;
            $(window).scrollTop(t);

            $("#" + id).click();
        }

        //打赏
        function shwoReward() {
            //获取贡献者信息
            var arr = '{"knowledgeId":"89f4b4c9-9807-4a65-83ba-ae6550234911"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngContributorView",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (result) {
                    result = eval('(' + result + ')');
                    if (result.Status == "Ok") {
                        var msgData = eval('(' + result.Data + ')');
                        if (msgData.userId == "" || msgData.userId == null) {
                            $("#divRewardFail").hide();
                            $("#divNoContributors").show();
                        } else {
                            $("#contributorImg").attr("src", msgData.headPictureUrl);
                            $("#divContributors").html(msgData.cnName);
                            $("#availablePoints").html(msgData.validPoints);
                            $("#contributeUserId").val(msgData.userId);
                            $("#divRewardFail").hide();
                            $("#divRewardTwo").hide();
                            $("#divRewardOne").show();
                        }
                    } else {
                        $("#divRewardOne").hide();
                        $("#divRewardTwo").hide();
                        $("#divRewardFail").hide();
                        $("#divNoContributors").show();
                    }
                    $("#divReward").show();
                },
                error: function (data) {

                }
            });
        }
