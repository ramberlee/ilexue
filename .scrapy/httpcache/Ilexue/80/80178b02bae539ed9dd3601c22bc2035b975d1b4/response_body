 //设置播放器播放事件监听函数。
 player.onPlay = function (event) {
 console.log("onPlay");

 
 var context="";
 var nowTime=parseInt(player.bdPlayer.getPosition());
 var vediolength=parseInt(player.bdPlayer.getDuration());
 context += "开始播放时间："+nowTime+"，用户开始播放事件，(vediolength)视频总时长："+vediolength;
