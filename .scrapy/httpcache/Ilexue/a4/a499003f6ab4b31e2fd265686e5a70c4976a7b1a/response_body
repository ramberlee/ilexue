<script type="text/javascript">
 //打赏
 function changeReward(){
	var points=$("div#divRewardOne li.active").html();
	var availiablePoints= $("#availablePoints").html();
	var knowledgeId=$("#hidKnowledgeID").val();
	if(parseInt(points)>parseInt(availiablePoints)){
		 showWarningMsg(jQuery.i18n.map["sty_msg_insufficientpointsavailable"]);
		 return false;
	}
	var arr='{"knowledgeId":"'+knowledgeId+'","points":"'+points+'"}';
	jQuery.ajax({
		 type: "POST",
		 contentType: "text/json",
		 url: "/kng/services/KngComService.svc/KnowledgeReward",
		 data: arr,
		 dataType: 'json',
		 cache: false,
		 success: function (result) {		 
		 result = eval('(' + result + ')');
		 if (result.Status == "Ok") {
		 	$("#divRewardOne").hide();
					$("#divRewardTwo").show();	 
		 } else {
 showWarningMsg(result.Data);		 
 $("#divRewardOne").hide();
					$("#divRewardFail").show();
		 }
		 },
		 error: function (data) {
		 
		 }
		});	
 }
 
 
 //关闭
 function closeDiv(id) {
 	$("#divReward").hide();
 } 
 
 //积分
 function checkLi(obj){
		$(obj).addClass("active");
		$("li").each(function(i,e){
			if(obj!=e){
				$(e).removeClass("active");
			}
		});
	}		
	
</script>

	
