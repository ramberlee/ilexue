        $(document).ready(function () {
            try {
                document.ondragstart = function () {
                    var evt = getEvent();
                    if (evt) {
                        if (evt.srcElement.tagName != "INPUT" && evt.srcElement.tagName != "TEXTAREA") return false;
                    }
                    return true;
                }
            } catch (e) { };
            var isShowStar = $("#hidIsShowStar").val();
            var userPhoto = $("#hidUserPhoto").val();
            SyncLoadHtmlContent2('divComment', 'KngCommentNew', 'MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', 'fc0ac098-19fe-49b8-9d41-9ab1b8baa8b3|%e9%87%91%e5%ad%97%e5%a1%94%e5%8e%9f%e7%90%86|DocumentKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|cae89804-ea78-4402-a291-ada1a551de96|刘林平|fc0ac098-19fe-49b8-9d41-9ab1b8baa8b3|b8ab0857-f3f8-4444-98b5-be7bd12e2498|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
