    <script type="text/javascript">
        (function () {
            var summary = $("#lisummary"),
                offset = summary.offset();
            $(".el-kng-pop").css({
                "left": offset.left - 175,
                "top": offset.top - 94
            });
        })();
        //我知道了
        function iKnow() {
            document.cookie = "El_49b04dcc-4796-437d-ba87-4a8eebf9a22c=" + "49b04dcc-4796-437d-ba87-4a8eebf9a22c; expires=Fri, 31 Dec 9999 23:59:59 GMT;path=/";
            $(".el-kng-summary-tip").hide();
            $(".window-overlay").hide();
        }

        //显示简介
        function showInstroduction() {
            $(".window-overlay").show();
            $(".el-kng-detail-dialog").show();
        }

        //关闭简介
        function closeInstroduction() {
            $(".window-overlay").hide();
            $(".el-kng-detail-dialog").hide();
        }
    </script>
    <script type="text/javascript">
        (function ($) {
            var wh = $(window).height();
            $(".el-doc-play").height(wh - 120);
        })(jQuery);

        function testTransition() {
            var _style = document.createElement("div").style;
            return "transition" in _style || "mozTransition" in _style ||
					"webkitTransition" in _style;
        }

        $("#li1").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area");
            if (testTransition()) {
                elem.toggleClass("el-play-right-active");
            } else {
                if (!elem.hasClass("el-play-right-active")) {
                    elem.animate({ "margin-right": "309px" }, 200, function () {
                        elem.addClass("el-play-right-active");
                    });

                    $(".el-play-area-right").animate({ "width": "309px" }, 200);
                } else {
                    elem.animate({ "margin-right": "49px" }, 200, function () {
                        elem.removeClass("el-play-right-active");
                    });
                    $(".el-play-area-right").animate({ "width": "49px" }, 200);
                }
            }
        });

        $(".el-area-left-tag").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area-left"), target = $(".el-play-area");
            if (testTransition()) {
                $(".el-play-area-left").toggleClass("el-play-left-active");
            } else {
                if (!elem.hasClass("el-play-left-active")) {
                    target.animate({ "margin-left": "260px" }, 200, function () {
                        elem.addClass("el-play-left-active");
                    });

                    elem.animate({ "width": "260px" }, 200);
                } else {
                    target.animate({ "margin-left": "28px" }, 200, function () {
                        elem.removeClass("el-play-left-active");
                    });
                    $(".el-play-area-left").animate({ "width": "28px" }, 200);
                }
            }

        });

        //打赏
        function shwoReward() {
            //获取贡献者信息
            var arr = '{"knowledgeId":"dd5da5b0-2517-4756-9d18-8f7f21bfed4c"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngContributorView",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (result) {
                    result = eval('(' + result + ')');
                    if (result.Status == "Ok") {
                        var msgData = eval('(' + result.Data + ')');
                        if (msgData.userId == "" || msgData.userId == null) {
                            $("#divRewardFail").hide();
                            $("#divNoContributors").show();
                        } else {
                            $("#contributorImg").attr("src", msgData.headPictureUrl);
                            $("#divContributors").html(msgData.cnName);
                            $("#availablePoints").html(msgData.validPoints);
                            $("#contributeUserId").val(msgData.userId);
                            $("#divRewardFail").hide();
                            $("#divRewardTwo").hide();
                            $("#divRewardOne").show();
                        }
                    } else {
                        $("#divRewardOne").hide();
                        $("#divRewardTwo").hide();
                        $("#divRewardFail").hide();
                        $("#divNoContributors").show();
                    }
                    $("#divReward").show();
                },
                error: function (data) {

                }
            });
        }
    </script>
    <script type="text/javascript">

        //下载列表刷新
        var scrollCount = 0;

        /*点击下载按钮下载列表页出现，点击其他区域，关闭列表页面*/
        $(document).ready(function () {
            //页面初始化时隐藏弹窗  
            $('#doc-download').hide();

            //窗口外点击事件  
            $("#el-download").click(function (e) {
                //先重新加载数据再显示
                $("#ulKngDownList").empty();
                scrollCount = 0;
                loadDownDocList(1);

                //阻止事件冒泡,防止波及本按钮  
                e.stopPropagation();
                //显示弹窗  
                $('#doc-download').show();
            });
        });
        $(document).click(function (e) {
            var download = $("#doc-download");
            //判断事件对象不是弹窗对象并且弹窗对象不包含事件对象  
            if (!download.is(e.target) && download.has(e.target).length == 0) {
                //则隐藏弹窗  
                download.hide(100);
            }
        });

        $("#el-download").click(function (e) {
            //阻止事件冒泡，防止波及本按钮  
            e.stopPropagation();
            //隐藏弹窗  
            $('#doc-download').hide(500);
        });

        $("#ulKngDownList").scroll(function () {
            var scrollTop = $(this).scrollTop();
            var divHeight = $(this).height();
            var totalHeight = document.getElementById("ulKngDownList").scrollHeight;
            if (scrollTop + divHeight + 50 > totalHeight) {
                scrollCount++;    //滚动次数累加
                var offs = scrollCount * 9 + 1;
                loadDownDocList(offs);
            }
        });
                     

     </script>

