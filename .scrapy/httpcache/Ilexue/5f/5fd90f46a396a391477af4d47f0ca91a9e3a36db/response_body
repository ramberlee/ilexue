 function initDocPlayer(data, sizewidth, sizeheight) {
//设置是否显示下载按钮
var orgid="35a1eb12-7592-48a2-9688-1563ccb6d2f5";
var m = getQueryString("m");
 if (m == 1&&orgid=="379c18f4-f888-43f4-ae11-ab5351ca0981" ) {
 $("#btndownload").addClass("hiddenelement"); 
 }


 var Cpn = jQuery("#hidViewSchedule").val();
 if (Cpn == "" || Cpn == "0") {
 Cpn = getCookie(CookieName);
 }

 var isHideZoom = false;
 var width = 1000;
 if (Cpn == "" || Cpn == "0") {
 Cpn = "1";
 }

 data.pn = Cpn;
 if (isJY == "1") {
 isHideZoom = true;
 width = 820;
 data.pn = 1;
 }

 var lastPage = 1;
 var allPage = data.pageSize;
 var m = '';
 if (m != "1") {
 try {
 window.parent.docLoaded(allPage);
 }
 catch (err) {
 //在此处理错误
 }
 }

 //初始化播放器对象
 player_doc = new GalleryViewer({
 data: data,
 //容器的id名称
 containerId: 'docplayercontainer',
 //播放器的宽度
 width: width,
 //起始页码
 startPage: parseInt(data.pn),
 //加载中图片显示的图像
 defaultBackground: 'le',
 //缩放级别
 fullscreenZoomLevel: 0,
 //图片的尺寸
 size: {
 width: parseInt(sizewidth), //api中imageWidth字段
 height: parseInt(sizeheight)//api中imageHeight字段
 },
 hideZoom: isHideZoom,
 //跑马灯配置
 marquee: {
 show: IsMarquee,
 text: '李杭燃（0000067301）非银金融客户',
 fontSize: parseInt(fontsize1),
 color: fontcolor,
 fontFamily: fontfamily
 },
 //水印配置
 watermark: {
 show: IsWaterMark,
 text: '李杭燃（0000067301）非银金融客户',
 fontSize: parseInt(fontsize2),
 color: fontcolor,
 fontFamily: fontfamily,
 multiple: true
 },
 onNavigate: function (index) {
 console.log('page index:' + index);
 jQuery("#hidViewSchedule").val(index);
 },
 onFullscreen: function (state) {
 console.log('fullscreen state:' + state);
 },
 onZoom: function (level) {
 console.log('zoom level:' + level);
 },
 onComplete: function () {
 }
 });

 //删除跑马灯
 player_doc.DOMModified = function () {
 var userid = '49b04dcc-4796-437d-ba87-4a8eebf9a22c';
 if (getCookie("DelCount_" + userid) == "2") {
 //第二次删除直接锁定退出
 delCookie("DelCount_" + userid);
 document.getElementById("btnLockOut").click();
 } else {
 CookieName = "DelCount_" + userid;
 var delCount = getCookie("DelCount_" + userid) == "" ? "0" : getCookie("DelCount_" + userid);
 var delSum = parseInt(delCount) + 1;
 setCookie(delSum.toString());
 CookieName = "Doc" + fileId;
 layer.open({
 type: 1, //Page层类型
 area: ['320px', '300px'],
 title: false,
 shade: 0.6, //遮罩透明度
 anim: 1, //0-6的动画形式，-1不开启
 closeBtn: 0, //不显示关闭按钮
 content: '<div class="window-overlay"></div><div class="el-white-notice-container"><div class="el-white-notice text-center pt45"><img class="mb30" src="http://stream1.qida.yunxuetang.com.cn/yxt/SystemFiles/Themes/elearning30/new/images/icons/warning_tip.png" width="100" height="100" /><div class="text-left text-color6 mb40 lh24 ph55">不允许修改或去除跑马灯，请不要再次尝试，否则账号将会被锁定！</div><button type="button" class="btn btn-primary w-120 btn-nm btn-radius-lg" onclick="window.location.reload();">知道了</button></div></div>'
 });
 }
 };
 }
 </script>
 <script type="text/javascript">
 //设置cookie
 function setCookie(cvalue) {
 var d = new Date();
 d.setTime(d.getTime() + (30 * 24 * 60 * 60 * 1000));
 var expires = "expires=" + d.toUTCString();
 document.cookie = CookieName + "=" + cvalue + "; " + expires;
 }

 //获取cookie
 function getCookie(cname) {
 var name = cname + "=";
 var ca = document.cookie.split(';');
 for (var i = 0; i < ca.length; i++) {
 var c = ca[i];
 while (c.charAt(0) == ' ') c = c.substring(1);
 if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
 }
 return "";
 }

 //删除cookies
 function delCookie(name) {
 var exp = new Date();
 exp.setTime(exp.getTime() - 1);
 var cval = getCookie(name);
 if (cval != null)
 document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString();
 }
 </script>
 </div>

