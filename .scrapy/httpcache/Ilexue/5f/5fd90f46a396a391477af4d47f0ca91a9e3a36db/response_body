        <div class="y-container1 clearfix ">
            <div class="el-course-detail-left">
                <ul class="el-course-detail-tab clearfix">
                    <li id="liComment" class="active" onclick="changeContentTab(this,0)"><span data-localize="comment_title_comment">评论</span></li>
                    
                    <li id="liQuestion" onclick="changeContentTab(this,1)"><span data-localize="common_lbl_q&amp;a">问答</span></li>
                    
                    <li id="liNote" onclick="changeContentTab(this,2)"><span data-localize="kng_lbl_note">笔记</span></li>
