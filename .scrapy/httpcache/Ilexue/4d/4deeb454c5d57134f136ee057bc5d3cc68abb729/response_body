 var fontfamily = '微软雅黑';
 if ('方正黑体' != '') { fontfamily = '方正黑体'; };
 var fontsize1 = '18';
 var fontsize2 = '24';
 if ('20' != '') { fontsize1 = '20'; fontsize2 = '20'; };
 var fontcolor = '#fef318';
 if ('rgba(169,169,169,1.00)' != '') { fontcolor = 'rgba(169,169,169,1.00)'; };

 var CookieName = "Doc" + fileId;
 var IsMarquee = 'false';
 if (IsMarquee.toLowerCase() != "true") {
 IsMarquee = false;
 }
 else {
 IsMarquee = true;
 }

 var IsWaterMark = 'false';
 if (IsWaterMark.toLowerCase() != "true") {
 IsWaterMark = false;
 }
 else {
 IsWaterMark = true;
 }

 //当且只有图片数据有数据时才去调用
 if (docObject.newPlayListItem != null) {
 yxtCommon.picLoad(function () {
 initDocPlayer(docObject.newPlayListItem.imageList, docObject.newPlayListItem.imageWidth, docObject.newPlayListItem.imageHeight);
 });
 }

 function initDocPlayer(data, sizewidth, sizeheight) {
//设置是否显示下载按钮
var orgid="35a1eb12-7592-48a2-9688-1563ccb6d2f5";
var m = getQueryString("m");
 if (m == 1&&orgid=="379c18f4-f888-43f4-ae11-ab5351ca0981" ) {
 $("#btndownload").addClass("hiddenelement"); 
 }


 var Cpn = jQuery("#hidViewSchedule").val();
 if (Cpn == "" || Cpn == "0") {
 Cpn = getCookie(CookieName);
 }

 var isHideZoom = false;
 var width = 1000;
 if (Cpn == "" || Cpn == "0") {
 Cpn = "1";
 }

 data.pn = Cpn;
 if (isJY == "1") {
 isHideZoom = true;
 width = 820;
 data.pn = 1;
 }

 var lastPage = 1;
 var allPage = data.pageSize;
 var m = '';
 if (m != "1") {
 try {
 window.parent.docLoaded(allPage);
 }
 catch (err) {
 //在此处理错误
 }
 }

 //初始化播放器对象
 player_doc = new GalleryViewer({
 data: data,
 //容器的id名称
 containerId: 'docplayercontainer',
 //播放器的宽度
 width: width,
 //起始页码
 startPage: parseInt(data.pn),
 //加载中图片显示的图像
 defaultBackground: 'le',
 //缩放级别
 fullscreenZoomLevel: 0,
 //图片的尺寸
 size: {
 width: parseInt(sizewidth), //api中imageWidth字段
 height: parseInt(sizeheight)//api中imageHeight字段
 },
 hideZoom: isHideZoom,
 //跑马灯配置
 marquee: {
 show: IsMarquee,
 text: '李杭燃（0000067301）非银金融客户',
 fontSize: parseInt(fontsize1),
 color: fontcolor,
 fontFamily: fontfamily
 },
 //水印配置
 watermark: {
 show: IsWaterMark,
 text: '李杭燃（0000067301）非银金融客户',
 fontSize: parseInt(fontsize2),
 color: fontcolor,
