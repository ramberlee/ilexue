        function testTransition() {
            var _style = document.createElement("div").style;
            return "transition" in _style || "mozTransition" in _style ||
					"webkitTransition" in _style;
        }

        $("#li1").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area");
            if (testTransition()) {
                elem.toggleClass("el-play-right-active");
            } else {
                if (!elem.hasClass("el-play-right-active")) {
                    elem.animate({ "margin-right": "309px" }, 200, function () {
                        elem.addClass("el-play-right-active");
                    });

                    $(".el-play-area-right").animate({ "width": "309px" }, 200);
                } else {
                    elem.animate({ "margin-right": "49px" }, 200, function () {
                        elem.removeClass("el-play-right-active");
                    });
                    $(".el-play-area-right").animate({ "width": "49px" }, 200);
                }
            }
        });

        $(".el-area-left-tag").on("click", function () {
            $(this).toggleClass("active");
            var elem = $(".el-play-area-left"), target = $(".el-play-area");
            if (testTransition()) {
                $(".el-play-area-left").toggleClass("el-play-left-active");
            } else {
                if (!elem.hasClass("el-play-left-active")) {
                    target.animate({ "margin-left": "260px" }, 200, function () {
                        elem.addClass("el-play-left-active");
                    });

                    elem.animate({ "width": "260px" }, 200);
                } else {
                    target.animate({ "margin-left": "28px" }, 200, function () {
                        elem.removeClass("el-play-left-active");
                    });
                    $(".el-play-area-left").animate({ "width": "28px" }, 200);
                }
            }

        });

        
    </script>
    <script type="text/javascript">

        //下载列表的文件数量
        var downFilesCount = 0;

        //下载管理列表
        function loadDownDocList(offset) {
            var arr = '{"limit":"9","offset":"' + offset + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngDownDocList",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data != "") {
                        var result = data.datas;

                        for (var i = 0; i < result.length; i++) {

                            var html = '<li id=' + result[i].id + ' class="doc-line-list clearfix">';

                            if (result[i].fileType == "PDF") {
                                html += '<span class="icon-Knowledge-pdf pull-left">';
                            } else if (result[i].fileType == "Image") {
                                html += '<span class="icon-Knowledge-image pull-left">';
                            }

                            html += '</span><div class="list-body pull-left ml10 mr20"><div class="ellipsis text-normal hand font-size-13" title="' + result[i].knowledgeName + '">' + result[i].knowledgeName + '</div><div class="font-size-13 text-grey clearfix el-list-date">' + result[i].updateDate + '</div></div>';

                            if (result[i].isDownload == "1") {    //是否已下载 0：未下载 1：已下载 
                                html += '<div class="pull-left font-size-13 clearfix el-download-status zhclass"><div class="el-downloading"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div><div class="text-grey el-have-downloaded">(' + jQuery.i18n.map['lbl_msg_download'] + ')</div></div>';    //已下载
                            } else {
                                if (result[i].status == "0") {    //状态 0：处理中 1：处理完成 2:处理失败
                                    html += '<div class="text-grey font-size-13 pull-left clearfix being-processed zhclass">' + jQuery.i18n.map['lbl_msg_processing'] + '</div>';    //处理中
                                } else if (result[i].status == "1") {
                                    html += '<div class="text-grey font-size-13 pull-left el-downloading clearfix zhclass"><a class="hand" onclick="RecordDownCount(\'' + result[i].id + '\',\'' + result[i].fileUrl + '\')">' + jQuery.i18n.map['opt_lbl_download'] + '</a></div>';
                                } else {
                                    html += '<div class="font-size-13 pull-left clearfix text-red zhclass">' + jQuery.i18n.map['lbl_msg_processfailed'] + '</div>';   //处理失败
                                }
                            }

                            html += '<div class="el-del pull-right mr10 hand" onclick="DeleteFile(\'' + result[i].id + '\')"></div></li>';

                            $("#ulKngDownList").append(html);
                        }

                        //如果是中文状态，下载按钮等样式离右边近一点
                        if ("zh" == "zh") {
                            $(".zhclass").addClass("ml30");
                        }

                        var filesCount = data.paging;
                        downFilesCount = filesCount.count;

                        if (downFilesCount > 99) {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html("99");     //超过99 只显示99
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").show();
                        } else if (downFilesCount == 0) {
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#hidDownCount").hide().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                            $("#hidAddIcon").hide();
                        } else {
                            $("#ulKngDownList").show();
                            $("#divNoMsg").hide();
                            $("#hidDownCount").show().html(downFilesCount);
                            $("#spNoMsg").removeClass("text-grey").addClass("text-info hand");
                            $("#hidAddIcon").hide();
                        }
                    }
                }
            });
        }

        //记录下载列表里的 知识是否已下载过
        function RecordDownCount(id, url) {
            window.location.href = url;
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/RecordDownCount",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {     //成功则页面有已下载标识
                        //console.log("记录次数");                       
                    } else {
                    }
                }
            });
        }

        //删除下载列表的文件
        function DeleteFile(id) {
            var arr = '{"fileID":"' + id + '"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteKngDownFile",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        $("#" + id + "").remove();
                        downFilesCount--;
                        if (downFilesCount > 99) {
                            $("#hidDownCount").html("99");
                            $("#hidAddIcon").show();
                        } else {
                            $("#hidDownCount").html(downFilesCount);
                            $("#hidAddIcon").hide();
                        }

                        if (downFilesCount == 0) {
                            $("#hidDownCount").hide()
                            $("#hidAddIcon").hide();
                            $("#ulKngDownList").hide();
                            $("#divNoMsg").show();
                            $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");
                        }
                    }
                }
            });
        }

        //清空下载列表
        function DeleteAllFiles() {
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/DeleteAllKngDownFiles",
                data: "",
                dataType: 'json',
                cache: false,
                success: function (data) {
                    if (data) {
                        downFilesCount = 0;
                        $("#hidDownCount").hide().html(downFilesCount);
                        $("#hidAddIcon").hide();
                        //显示 暂无数据
                        $("#ulKngDownList").hide().empty();
                        $("#divNoMsg").show();
                        $("#spNoMsg").removeClass("text-info hand").addClass("text-grey");     //置灰
                    }
                }
            });
        }

        //根据是否开启机构参显示右上角图标
        function showDownLoad() {
            if ($("#hidKngDownType").val() == "1") {
                $("#divDownLoad").removeClass("hidden");
                $("#el-download").show();
            } else {
                $("#el-download").hide();
            }
        }

    </script>
    <script type="text/javascript">

        //下载列表刷新
        var scrollCount = 0;

        /*点击下载按钮下载列表页出现，点击其他区域，关闭列表页面*/
        $(document).ready(function () {
            //页面初始化时隐藏弹窗  
            $('#doc-download').hide();

            //窗口外点击事件  
            $("#el-download").click(function (e) {
                //先重新加载数据再显示
                $("#ulKngDownList").empty();
                scrollCount = 0;
                loadDownDocList(1);

                //阻止事件冒泡,防止波及本按钮  
                e.stopPropagation();
                //显示弹窗  
                $('#doc-download').show();
            });
        });

        $(document).click(function (e) {
            var download = $("#doc-download");
            //判断事件对象不是弹窗对象并且弹窗对象不包含事件对象  
            if (!download.is(e.target) && download.has(e.target).length == 0) {
                //则隐藏弹窗  
                download.hide(100);
            }
        });

        $("#el-download").click(function (e) {
            //阻止事件冒泡，防止波及本按钮  
            e.stopPropagation();
            //隐藏弹窗  
            $('#doc-download').hide(500);
        });

        $("#ulKngDownList").scroll(function () {
            var scrollTop = $(this).scrollTop();
            var divHeight = $(this).height();
            var totalHeight = document.getElementById("ulKngDownList").scrollHeight;
            if (scrollTop + divHeight + 50 > totalHeight) {
                scrollCount++;    //滚动次数累加
                var offs = scrollCount * 9 + 1;
                loadDownDocList(offs);
            }
        });

        //显示默认图
        function showDefaultImg() {
            $("#divDefaultImg").removeClass("hidden");
        }

        //有讲义的音频默认图样式
        function shouwJYImgCss() {
            $("#divDefaultImg").removeClass("jw-center-tu").addClass("jw-center-tu-packge");
            $("#divDefaultImg img").addClass("jw-center-img").removeAttr("height").removeAttr("width");
            showDefaultImg();
        }

     </script>
    
