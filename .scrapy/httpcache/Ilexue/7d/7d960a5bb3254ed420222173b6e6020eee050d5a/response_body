<script type="text/javascript">

 function showCantPlay2(type) {
 addFilter("dvCantPlayFilter");
 var left = ($(document).width() - 600) / 2;
 //var top = $(document).scrollTop() + 180;
 $("#dvCantPlay2").css("left", left + "px").css("top", 300 + "px");
 var orderMsg = $("#ordermsg");
 if (type == 0) {
 orderMsg.html(jQuery.i18n.map['sty_msg_cannotlearn']);
 } else {
 orderMsg.html(jQuery.i18n.map['sty_msg_afterassessmentlearn']);
 }
 $("#dvCantPlay2").show();
 }

 function closeCantPlay2() {
 $("#dvCantPlay2").hide();
 closeFilter("dvCantPlayFilter");
 }

 //单击学习行数据
 function StudyRowClick(url, type, examstatus, allowview, iscanstudy, iscanexam) {

 if (type == "OteExam") {
 if (url.indexOf('MasterType=PlanStudy') < 0) {
 url = url.replace("MasterType=Plan", "MasterType=PlanStudy");
 }
 if (url.indexOf('MasterType=PositionStudy') < 0) {
 url = url.replace("MasterType=Position", "MasterType=PositionStudy");
 }
 }

 //知识是否已过期
 var isexpired = $("#hidIsExpire").val();
 if (isexpired == "true") {
 showWarningMsg(jQuery.i18n.map['sys_msg_pageerrormsg21']);
 return false;
 }
 //是否需要积分
 var isneedpoint = $("#hidIsNeedPoint").val()
 if (isneedpoint == "true") {
 showWarningMsg(jQuery.i18n.map['sty_lbl_buythereartolearn']);
 return false;
 }

 var m = getQueryString("m");
 if (m == 1) {
 if (url.indexOf("?") == -1) {
 url = url + "?m=1";
 }
 else {
 url = url + "&m=1";
 }
 }
 var mh = getQueryString("mh");
 if (mh == 1) {
 if (url.indexOf("?") == -1) {
 url = url + "?mh=1";
 } else {
 url = url + "&mh=1";
 }
 }

 var taskId=getQueryString("taskId");
 if (taskId !="") {
 if (url.indexOf("?") == -1) {
 url = url + "?taskId="+taskId;
 } else {
 url = url + "&taskId="+taskId;
 }
 }

var st=getQueryString("st");
 if (st!="") {
 if (url.indexOf("?") == -1) {
 url = url + "?st="+st;
 } else {
 url = url + "&st="+st;
 }
 }

var objectId=getQueryString("objectId");
 if (objectId!="") {
 if (url.indexOf("?") == -1) {
 url = url + "?objectId="+objectId;
 } else {
 url = url + "&objectId="+objectId;
 }
 }

 if (iscanexam == "False") {
 showCantPlay2(1);
 return false;
 } else if (iscanstudy == "False") {
 showCantPlay2(0);
 return false;
 }
 if (type == "OteExam") {
 if (m == 1) {
 showWarningMsg(jQuery.i18n.map['sty_lbl_cannotexam']);
 }
 else {
 if (examstatus == "Marking" || examstatus == "Assessment") {
 showWarningMsg(jQuery.i18n.map['sty_lbl_review']);
 }
 else {
 openURL(url);
 }
 }
 } else {
 openURL(url);
 }
 }
 //重新考试
 function ResetExam(obj, url) {
 openURL(url);
 stopPropagation(obj);
 }

 //查看结果
 function ViewExam(obj, id) {
 var url = '/exam/examquestionpreview.htm?UserExamMapID=' + id;
 openURL(url);
 stopPropagation(obj);
 }

 //阻止冒泡
 function stopPropagation(e) {
 e = e || window.event;
 if (e.stopPropagation) { //W3C阻止冒泡方法
 e.stopPropagation();
 } else {
 e.cancelBubble = true; //IE阻止冒泡方法 
 }
 }
</script>

