 //发送消息
 function sendMessageWithNewLj() {
 var isself = false;
 if (jQuery("#chbPressToMe01").prop("checked")) {
 isself = true;
 }
 else {
 isself = false;
 }
 var ShowMessage="";
 var type = jQuery("#hidsendtype01").val();
 if (type == "AppsNoInstallation") {
 ShowMessage = jQuery.i18n.map['sys_lbl_promptsend']; 
 }
 else {
 ShowMessage = jQuery.i18n.map['sys_lbl_remindermessagesend'];
 }
 var content = jQuery("#txtPress01").val();
 var arr = '{"orgID":"$FunctionLabel.AppContext.CurrentOrgID","userID":"49b04dcc-4796-437d-ba87-4a8eebf9a22c","cnName":"李杭燃","content":"' + content + '","type":"' + type + '","isself":"' + isself + '"}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/CommonService.svc/SendStatisticMessage",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 showMsgSuccessPanel(jQuery('#dvnotice01'), ShowMessage);
 jQuery("#dvnotice01").hide();
 },
 error: function (XMLHttpRequest, textStatus, errorThrown) {
 }
 });
 }
 //关闭提醒窗口
 function closeEmpPop3WithNewLj() {
 jQuery("#dvnotice01").hide();
 }
