 function openDivNoticeWithNew() {
 var left = (jQuery(document).width() - 500) / 2;
 var top = (jQuery(window).height() - 257) / 2;
 jQuery("#dvnotice01").css("top", top + "px").css("left", left + "px").show();
 addFilter("dvFilter1","603");
 }
