 //设置播放器播放事件监听函数。
 player.onPlay = function (event) {
 console.log("onPlay");

 
 var context="";
 var nowTime=parseInt(player.bdPlayer.getPosition());
 var vediolength=parseInt(player.bdPlayer.getDuration());
 context += "开始播放时间："+nowTime+"，用户开始播放事件，(vediolength)视频总时长："+vediolength;
 CreateActionLog("play",context);

 vediolength=parseInt(player.bdPlayer.getDuration());
 if(vediolength<15)
 {
 completedLaveTimeStudy();
 }


 if (isFirstLoad) {
 isFirstLoad = false;
 TimerStartLoad();
 }else{
 ResetTimer();
 }
 //加载完播放器之后，在检查是否防作弊
 if (jQuery("#hfTipBytitle").val() != "") {
 showSingleTrack(jQuery("#hfTipBytitle").val());
 jQuery("#hfTipBytitle").val('');
 }
 };

 //设置播放器暂停事件监听函数。
 player.onPause = function (event) {

 var context="";
 var vediolength=parseInt(player.bdPlayer.getDuration());
 var nowTime=parseInt(player.bdPlayer.getPosition());
 context += "用户暂停时间："+nowTime+"，用户暂停事件，(vediolength)视频总时长："+vediolength;
 CreateActionLog("pause",context);

 console.log("onPause");
 if (timer) {
 clearInterval(timer);
 }
 };

 player.onError = function (event) {
 console.log("Error");
 };

 //设置播放器就绪事件监听函数。
 player.onReady = function (event) {
 console.log("onReady");
 };

 //设置播放器窗口大小变化监听函数。
 player.onResize = function (event) {
 console.log("onResize");
 };

 //设置播放器拖动事件监听函数。
 player.onSeek = function (event) {

 var context="";
 var vediolength=parseInt(player.bdPlayer.getDuration());
 context += "(源播放时间)sourceTime：" + parseInt(event.position) + "，(目标播放时间)targesTime："+parseInt(event.offset)+"，(视频时长)VideoLength："+vediolength;
 CreateActionLog("seek",context); 

 console.log("onSeek");
 };
 var lastShowTime = 0;
 //设置播放器播放进度变化事件监听函数。
 player.onTime = function (event) {
 console.log("onTime "+ myPlayer.getPosition());
 jQuery("#hidViewSchedule").val(myPlayer.getPosition());
 var curPosition = myPlayer.getPosition();
 };

 //设置播放器全屏状态变化监听函数。
 player.onFullscreen = function (event) {
