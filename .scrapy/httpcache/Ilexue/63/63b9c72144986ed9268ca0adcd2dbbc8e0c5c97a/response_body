 function videoDetail(id) {
 if ('' == "1") {
 if ('0' == "1") {
 ShowIFrameDialog("知识编辑", 964, 760, "/kng/Upload/KnowledgeEditLocal.aspx?id=" + id + "&OP=Edit");
