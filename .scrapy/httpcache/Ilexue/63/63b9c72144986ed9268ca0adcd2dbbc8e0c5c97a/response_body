 ShowIFrameDialog("知识编辑", 964, 550, "/kng/Upload/ShareEditMix.aspx?id=" + id + "&OP=Edit");
 }
 else {
 ShowIFrameDialog("知识编辑", 964, 550, "/kng/Upload/ShareEdit.aspx?id=" + id + "&OP=Edit");
 }
 }
 return false;
 }
