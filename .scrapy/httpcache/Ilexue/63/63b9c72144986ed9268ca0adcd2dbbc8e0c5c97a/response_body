 //设置播放器完成事件监听函数。
 player.onComplete = function (event) {
 
 if(!firstComplete)
 {

 jQuery("#hidViewSchedule").val("0");
 delCookie(CookieName);

var sTime=standardStudyHours-actualStudyHours;

 if(sTime>120) {
	//StopTimer();
	setTimeout(StopTimer, 1000);
 }

 if("1"=="0")
 {
 return;
 }

 if(""=="1")
 {
 
 firstComplete=true;
 window.open("",'_self')

 }else{

 var schedule=$("#ScheduleText").text();
 var vediolength=parseInt(player.bdPlayer.getDuration());
 if(vediolength<15 && schedule.indexOf('100')==-1)
 {
 completedLaveTimeStudy();
 }

 
 //如果倒计时小于60s（备注：断点续播时，视频播放时间小于120秒，导致播放完成不能提交进度）
 try {
 var laveTime=standardStudyHours-actualStudyHours;
 if(laveTime<120 && schedule.indexOf('100')==-1)
 {
 completedLaveTimeStudy();
 }
 } catch(ex) {
 console.log("进度100%");
 }

 if("".toLowerCase()=="true")
 {
 //控制学习顺序，到100%时自动续播
 if(schedule.indexOf('100')>-1)
 {
 firstComplete=true;
 window.open("",'_self');
 }
 }else{
 firstComplete=true;
 window.open("",'_self');
 }

 }

 }

 };

 //设置切换码率时事件监听函数。
 player.onQualityChange = function (event) {
 console.log("onQualityChange");
 };

//用户通过F12删除跑马灯或者广告后触发,count是用户删除或者编辑DOM的次数，可根据具体业务进行控制
player.DOMModified = function (count) {
var userid="49b04dcc-4796-437d-ba87-4a8eebf9a22c";
if(getCookie("DelCount_"+userid)=="2"){
//第二次删除直接锁定退出
 delCookie("DelCount_"+userid);
 jQuery("#btnLockOut").click();
}else{
 CookieName = "DelCount_" + userid;
 var delCount=getCookie("DelCount_" + userid)==""?"0":getCookie("DelCount_" + userid);
 var delSum = parseInt(delCount)+1;
 setCookie(delSum.toString());
 CookieName = "Video"+MasterType + fileId;
layer.open({
 type: 1 //Page层类型
 ,title: false
 ,area: ['320px', '300px']
 ,shade: 0.6 //遮罩透明度
 ,anim: 1 //0-6的动画形式，-1不开启
 ,closeBtn: 0 //不显示关闭按钮
 ,content: '<div class="window-overlay"></div><div class="el-white-notice-container"><div class="el-white-notice text-center pt45"><img class="mb30" src="http://stream1.qida.yunxuetang.com.cn/yxt/SystemFiles/Themes/elearning30/new/images/icons/warning_tip.png" width="100" height="100" /><div class="text-left text-color6 mb40 lh24 ph55">不允许修改或去除跑马灯，请不要再次尝试，否则账号将会被锁定！</div><button type="button" class="btn btn-primary w-120 btn-nm btn-radius-lg" onclick="window.location.reload();">知道了</button></div></div>'
}); 
}
};

//由于跑马灯和水印不支持ie8，所以如果是IE8浏览器，可以在下面的方法中进行提醒
 //=============================================
 })

</script>
<script type="text/javascript">
 //设置cookie
 function setCookie(cvalue) {
 var d = new Date();
 d.setTime(d.getTime() + (30 * 24 * 60 * 60 * 1000));
 var expires = "expires=" + d.toUTCString();
 document.cookie = CookieName + "=" + cvalue + "; " + expires;
 }

 //获取cookie
 function getCookie(cname) {
 var name = cname + "=";
 var ca = document.cookie.split(';');
 for (var i = 0; i < ca.length; i++) {
 var c = ca[i];
 while (c.charAt(0) == ' ') c = c.substring(1);
 if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
 }
 return "";
 }

 //删除cookies
 function delCookie(name) {
 var exp = new Date();
 exp.setTime(exp.getTime() - 1);
 var cval = getCookie(name);
 if (cval != null)
 document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString();
 } 

 //播放视频的行为日志 创建者：hhb 时间：2019-11-19 20:00:37（己亥 十月二十三）
 function CreateActionLog(type, context) {

 var methodName = 'CreateActionLog';
 var kngId = $("#hidKnowledgeID").val();
 var bachNo = $("#hidBachNo").val();

 var arr = '{"kngId":"' + kngId + '","bachNo":"' + bachNo+ '","type":"' + type + '","context":"' + context + '"}';
 AjaxAsyncCallMethodWithKng(methodName, arr,
 function (result) {
 console.log("客户操作视频播放器，写入日志成功！");
 }, true, true, true);
 }

</script>
