 CookieName = "DelCount_" + userid;
 var delCount=getCookie("DelCount_" + userid)==""?"0":getCookie("DelCount_" + userid);
 var delSum = parseInt(delCount)+1;
 setCookie(delSum.toString());
 CookieName = "Video"+MasterType + fileId;
layer.open({
 type: 1 //Page层类型
 ,title: false
 ,area: ['320px', '300px']
 ,shade: 0.6 //遮罩透明度
 ,anim: 1 //0-6的动画形式，-1不开启
 ,closeBtn: 0 //不显示关闭按钮
 ,content: '<div class="window-overlay"></div><div class="el-white-notice-container"><div class="el-white-notice text-center pt45"><img class="mb30" src="http://stream1.qida.yunxuetang.com.cn/yxt/SystemFiles/Themes/elearning30/new/images/icons/warning_tip.png" width="100" height="100" /><div class="text-left text-color6 mb40 lh24 ph55">不允许修改或去除跑马灯，请不要再次尝试，否则账号将会被锁定！</div><button type="button" class="btn btn-primary w-120 btn-nm btn-radius-lg" onclick="window.location.reload();">知道了</button></div></div>'
}); 
}
