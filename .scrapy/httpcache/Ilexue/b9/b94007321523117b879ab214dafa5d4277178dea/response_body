            $(window).scrollTop(t);

            $("#" + id).click();
        }

        //打赏
        function shwoReward() {
            //获取贡献者信息
            var arr = '{"knowledgeId":"dc7c8baa-18ae-4c21-a5d1-51fb4d8b2915"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngContributorView",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (result) {
                    result = eval('(' + result + ')');
                    if (result.Status == "Ok") {
                        var msgData = eval('(' + result.Data + ')');
                        if (msgData.userId == "" || msgData.userId == null) {
                            $("#divRewardFail").hide();
                            $("#divNoContributors").show();
                        } else {
                            $("#contributorImg").attr("src", msgData.headPictureUrl);
                            $("#divContributors").html(msgData.cnName);
                            $("#availablePoints").html(msgData.validPoints);
                            $("#contributeUserId").val(msgData.userId);
                            $("#divRewardFail").hide();
                            $("#divRewardTwo").hide();
                            $("#divRewardOne").show();
                        }
                    } else {
                        $("#divRewardOne").hide();
                        $("#divRewardTwo").hide();
                        $("#divRewardFail").hide();
                        $("#divNoContributors").show();
                    }
                    $("#divReward").show();
                },
                error: function (data) {

                }
            });
        }
