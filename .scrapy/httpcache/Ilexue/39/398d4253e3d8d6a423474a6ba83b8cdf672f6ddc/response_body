        //打赏
        function shwoReward() {
            //获取贡献者信息
            var arr = '{"knowledgeId":"5014143e-dcd0-424c-b818-09bbf460b0cb"}';
            jQuery.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/GetKngContributorView",
                data: arr,
                dataType: 'json',
                cache: false,
                success: function (result) {
                    result = eval('(' + result + ')');
                    if (result.Status == "Ok") {
                        var msgData = eval('(' + result.Data + ')');
                        if (msgData.userId == "" || msgData.userId == null) {
                            $("#divRewardFail").hide();
                            $("#divNoContributors").show();
                        } else {
                            $("#contributorImg").attr("src", msgData.headPictureUrl);
                            $("#divContributors").html(msgData.cnName);
                            $("#availablePoints").html(msgData.validPoints);
                            $("#contributeUserId").val(msgData.userId);
                            $("#divRewardFail").hide();
                            $("#divRewardTwo").hide();
                            $("#divRewardOne").show();
                        }
                    } else {
                        $("#divRewardOne").hide();
                        $("#divRewardTwo").hide();
                        $("#divRewardFail").hide();
                        $("#divNoContributors").show();
                    }
                    $("#divReward").show();
                },
                error: function (data) {

                }
            });
        }
