        }

        function js_getSessionInfo() {
            var kid = '5014143e-dcd0-424c-b818-09bbf460b0cb';
            var userid = '49b04dcc-4796-437d-ba87-4a8eebf9a22c';
            var timestamp = new Date().valueOf();
            return "" + userid + "|" + kid + "|" + timestamp + "";    //用户ID和视频ID或课程ID,时间戳
        }

