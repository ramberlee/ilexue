 }

 //当且只有图片数据有数据时才去调用
 if (docObject.newPlayListItem != null) {
 yxtCommon.picLoad(function () {
 initDocPlayer(docObject.newPlayListItem.imageList, docObject.newPlayListItem.imageWidth, docObject.newPlayListItem.imageHeight);
 });
 }

 function initDocPlayer(data, sizewidth, sizeheight) {
//设置是否显示下载按钮
var orgid="35a1eb12-7592-48a2-9688-1563ccb6d2f5";
var m = getQueryString("m");
