 player_doc.DOMModified = function () {
 var userid = '49b04dcc-4796-437d-ba87-4a8eebf9a22c';
 if (getCookie("DelCount_" + userid) == "2") {
 //第二次删除直接锁定退出
 delCookie("DelCount_" + userid);
 document.getElementById("btnLockOut").click();
 } else {
 CookieName = "DelCount_" + userid;
 var delCount = getCookie("DelCount_" + userid) == "" ? "0" : getCookie("DelCount_" + userid);
 var delSum = parseInt(delCount) + 1;
 setCookie(delSum.toString());
