<script type="text/javascript">
 var iscurfinish;

 function showCantPlay2(type, nextID) {
 var orderMsg = $("#ordermsg");
 if (!checkNextKng(nextID)) {
 if (type == 0) {
 orderMsg.html("暂无法学习此知识。此知识受学习顺序控制，您需要在完成前面知识的学习后才可以学习此知识。");
 } else {
 orderMsg.html("暂无法学习此知识。此知识受学习顺序控制，您需要在通过之前的测评后才可以学习此知识。");
 }
 $("#dvCantPlay2").show();
 return false;
 }
 return true;
 }

 function closeCantPlay2() {
 $("#dvCantPlay2").hide();
 }

 //单击学习行数据
 function StudyRowClick(url, type, examstatus, allowview, iscanstudy, iscanexam, nextID) {
 var m = getQueryString("m");
 if (m == 1) {
 if (url.indexOf("?") == -1) {
 url = url + "?m=1";
 }
 else {
 url = url + "&m=1";
 }

 }
 if (url.indexOf("plan") > -1) {
 var studyPhaseId = $("#" + nextID).attr("phaseid");

 var arr = '{"studyPlanId":"","studyPlanPhaseId":"' + studyPhaseId + '","knowledgeId":"' + nextID + '"}';

 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/kng/services/KngComService.svc/CheckStyControlSequence",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 var result = eval('(' + result + ')');
 if (result.Status != "OK") {
 //需要控制顺序
 showWarningMsg(result.Data);
 return false;
 }
 else {
 StudyRowClickOthers(url, type, examstatus, allowview, iscanstudy, iscanexam, nextID);
 }
 }
 });
 }
 else {
 StudyRowClickOthers(url, type, examstatus, allowview, iscanstudy, iscanexam, nextID);
 }


 }

 function checkNextKng(kngId) {
 var schedule = $("#ScheduleText").text();
 if (schedule != "100%") {
 return false;
 }
 var sourceId = $(".select").attr("id");
 var nextId = $("#" + sourceId).next().attr("id");
 if (!nextId) {
 nextId = $("#" + sourceId).next().next().attr("id");
 }

 if (nextId != kngId) {
 return false;
 }
 return true;
 }

 function StudyRowClickOthers(url, type, examstatus, allowview, iscanstudy, iscanexam, nextID) {
 var m = getQueryString("m");
 var st = getQueryString("st");
 var masterId = getQueryString("MasterID");
 var objectId = getQueryString("objectId");
 url += url.indexOf("?") < 0 ? "?" : "&";
 console.log(url);
 if (st) {
 url += "st=" + st;
 }

 if (masterId && url.indexOf("MasterID") < 0) {
 url += "&MasterID=" + masterId
 }

 if (objectId && url.indexOf("objectId") < 0) {
 url += "&objectId=" + objectId;
 }

 if (url.indexOf("package") > -1 && url.indexOf("kng/course/package") < 0 && url.indexOf("packageid") < 0) {
 url = url.replace("package", "kng/course/package");
 }

 var ispass = 0;
 //学习跟踪 判断是否从未完成到已完成
 if (iscurfinish != undefined && iscurfinish == 1) {
 //同步当前单击就是当前知识的下一个知识
 if ($("input[name=hfNext]").val() == nextID) {
 ispass = 1;
 }
 }
 if (ispass != 1) {
 if (iscanexam == "False") {
 if (!showCantPlay2(1, nextID)) {
 return false;
 };
 } else if (iscanstudy == "False") {
 //判断当前的控制学习
 if (!showCantPlay2(0, nextID)) {
 return false;
 };
 }
 }
 if (type == "OteExam") {
 if (m == 1) {
 showWarningMsg("当前预览状态，不能参加考试。");
 }
 else {
 if (examstatus == "Marking" || examstatus == "Assessment") {
 showWarningMsg("当前测评正在批阅中……");
 } else if (examstatus == "Done" && allowview == "False") {
 showWarningMsg("当前测评已完成，暂不支持查看试卷结果。");
 } else {
 openURL(url);
 }
 }
 } else {
 var arr = '{"userID":"49b04dcc-4796-437d-ba87-4a8eebf9a22c"}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/StyService.svc/RemoveUserTrackKnowledgeID",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 }
 });
 window.location.href = url;
 }
 }
</script>

