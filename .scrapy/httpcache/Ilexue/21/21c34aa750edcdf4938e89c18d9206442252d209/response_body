    <script type="text/javascript" src="https://staticbd.yunxuetang.cn/yxt/SystemFiles/Js/raphael.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var m = getQueryString("m");
            if (m == 1) {
                $("#divStudySchedule").hide();
            } else {
                var masterType = '';
                //检查版本除（学习计划）
                if (masterType != 'Plan') {
                    var packId = $("#hidSourceID").val();
                    var type = "";
                    var masterId = "";
                    if (masterType == "Position") {
                        type = "Position";
                        masterId = '';
                    }
                    var arr = '{"packageId":"' + packId + '","masterId":"' + masterId + '","masterType":"' + type + '"}';
                    $.ajax({
                        type: "POST",
                        contentType: "text/json",
                        url: "/kng/services/KngComService.svc/GetCheckCoursePackageVersion",
                        dataType: 'json',
                        data: arr,
                        cache: false,
                        success: function (result) {
                            if (result != "") {
                                var advData = eval('(' + result + ')');
                                if (advData.hasNewVersion) {
                                    //有大版本更新
                                    showOverCourse();
                                }
                            }
                        }
                    });
                }
            }

            var isneedpoint = $("#hidIsNeedPoint").val();
            if ('0' == '1') {
                $("#liFavorite2").prop("class", "hidden");
                $("#liFavorite").prop("class", "hidden");
            } else {
                if ($("#hfIsFavorite").val() == "1") {
                    if (isneedpoint == "true") {
                        $("#collect2").text(jQuery.i18n.map['common_lbl_alreadyfavourite']);
                        $("#liFavorite2").prop("class", "el-btn-collection ml15 mt10 active");
                    } else {
                        $("#collect").text(jQuery.i18n.map['common_lbl_alreadyfavourite']);
                        $("#liFavorite").prop("class", "el-btn-collection ml15 mt20 active");
                    }
                } else {
                    if (isneedpoint == "true") {
                        $("#collect2").text(jQuery.i18n.map['common_lbl_collect']);
                        $("#liFavorite2").prop("class", "el-btn-collection ml15 mt10");
                    } else {
                        $("#collect").text(jQuery.i18n.map['common_lbl_collect']);
                        $("#liFavorite").prop("class", "el-btn-collection ml15 mt20");
                    }
                }
            }
            var isShowStar = $("#hidIsShowStar").val();
            var userPhoto = $("#hidUserPhoto").val();

            GetDataByLabelENameWithKng('commentDiv', 'KngCommentNew', 0, 0, 'HiddenCommentArea|MasterID|MasterName|CommentType|UserID|UserPhoto|CommentedUserID|CommentedCName|SourceID|PackageID|OrgID|IsShowStar|IsApply', '0|0d8ba957-07c9-4719-a63a-b438408a3349|2020年数字营销业务落地最佳实践|CourseKnowledge|49b04dcc-4796-437d-ba87-4a8eebf9a22c|' + userPhoto + '|df8c0cf5-bc04-41da-9208-93e3ef3ec36a|史铭芳|0d8ba957-07c9-4719-a63a-b438408a3349|0d8ba957-07c9-4719-a63a-b438408a3349|35a1eb12-7592-48a2-9688-1563ccb6d2f5|' + isShowStar + '|1');
        });

        //0: 帖子 1:最红 单击Tab选项卡
        function changeExpertTab(index) {
            if (index == 0) {  //课程目录
                $("#divPublishComment").addClass("hidden");
                $("#cchapter").addClass("active");
                $("#ccommet").removeClass("active");
                $("#cintroduction").removeClass("active");
                $("#divcourselist").show();
                $("#commentDiv").hide();
                $("#divIntroduction").hide();
                $("#courseTitle").hide();
                $("#commentTitle").hide();
            } else if (index == 1) {//评论
                $("#divPublishComment").removeClass("hidden");
                $("#cchapter").removeClass("active");
                $("#ccommet").addClass("active");
                $("#cintroduction").removeClass("active");
                $("#divcourselist").hide();
                $("#commentDiv").show();
                $("#divIntroduction").hide();
                $("#courseTitle").hide();
                $("#commentTitle").hide();
            } else if (index == 2) { //介绍
                $("#divPublishComment").addClass("hidden");
                $("#cchapter").removeClass("active");
                $("#ccommet").removeClass("active");
                $("#cintroduction").addClass("active");
                $("#divIntroduction").show();
                $("#courseTitle").show();
                $("#commentTitle").show();
                $("#divcourselist").show();
                $("#commentDiv").show();
            }
        }

        //收藏
        function CheckAddToFavorite() {
            var knowledgeID = $.trim("0d8ba957-07c9-4719-a63a-b438408a3349");
            var packageID = $.trim(" ");
            var masterID = $.trim("");
            var masterType = $.trim("");
            var isneedpoint = $("#hidIsNeedPoint").val();
            if ($('#hfIsFavorite').val() == "1") {
                var arr = '{"knowledgeID":"' + knowledgeID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/DeleteFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")");
                        if (result.Status == "OK") {
                            if (isneedpoint == "true") {
                                $("#collect2").text(jQuery.i18n.map['common_lbl_collect']);
                                $("#liFavorite2").prop("class", "el-btn-collection ml15 mt10");
                            } else {
                                $("#collect").text(jQuery.i18n.map['common_lbl_collect']);
                                $("#liFavorite").prop("class", "el-btn-collection ml15 mt20");
                            }

                            $("#hfIsFavorite").val("0");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_cancelcollectionsuccess']);
                        }
                        else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            }
            else {
                var arr = '{"knowledgeID":"' + knowledgeID + '","packageID":"' + packageID + '","masterType":"' + masterType + '","masterID":"' + masterID + '"}';
                jQuery.ajax({
                    type: "POST",
                    contentType: "text/json",
                    url: "/kng/services/KngComService.svc/AddFavorite",
                    data: arr,
                    dataType: 'json',
                    cache: false,
                    success: function (result) {
                        result = eval("(" + result + ")");
                        if (result.Status == "OK") {

                            if (isneedpoint == "true") {
                                $("#collect2").text(jQuery.i18n.map['common_lbl_alreadyfavourite']);
                                $("#liFavorite2").prop("class", "el-btn-collection ml15 mt10 active");
                            } else {
                                $("#collect").text(jQuery.i18n.map['common_lbl_alreadyfavourite']);
                                $("#liFavorite").prop("class", "el-btn-collection ml15 mt20 active");
                            }

                            $("#hfIsFavorite").val("1");
                            showSuccessMsg(jQuery.i18n.map['kng_lbl_collectionsuccess']);
                        }
                        else {
                            showWarningMsg(result.Data);
                        }
                    }
                });
            }
            return false;
        }

        //课程兑换
        function shwoExchange() {
            var isexpired = $("#hidIsExpire").val();
            if (isexpired == "true") {
                showWarningMsg(jQuery.i18n.map['sys_msg_pageerrormsg21']);
                return false;
            }
            $("#divExchange").show();
            $("#divKnowledgeDetail").show();
        }

        //生成版本升级提醒
        function showOverCourse() {
            var html = "<div class='window-overlay'></div>" +
 	                        "<div class='el-white-notice-container'>" +
 		                        "<div class='el-white-notice text-center pt45' style='width: 400px;height:400px;border-radius: 0;'>" +
 			                        "<img class='mb10' src='https://staticbd.yunxuetang.cn/yxt/SystemFiles/Themes/elearning30/new/images/background/course_update.jpg' />" +
 			                        "<div class='text-color6 pb50 lh18 ph55'>" + jQuery.i18n.map['kng_lbl_coursechangewarm'] + "</div>" +
 			                        "<button id='btnGoHead' type='button' style='width:232px;' class='btn btn-primary mt15 btn-nm' onclick='hideOverCourse()'>" + jQuery.i18n.map['sty_btn_goahead'] + "</button>" +
 			                        "<div class='text-primary mt20 hand' onclick='CloseCourseView();'>" + jQuery.i18n.map['common_btn_close'] + "</div>" +
 		                        "</div>" +
 	                        "</div>";
            $("#divOverCourse").html(html);
        }

        //隐藏版本升级提醒且同步版本
        function hideOverCourse() {
            $("#btnGoHead").prop("disabled", true);
            var masterType = '';
            var packId = $("#hidSourceID").val();
            var type = "";
            var masterId = "";
            if (masterType == "Position") { type = "Position"; masterId = ''; }
            var arr = '{"packageId":"' + packId + '","masterId":"' + masterId + '","masterType":"' + type + '"}';
            $.ajax({
                type: "POST",
                contentType: "text/json",
                url: "/kng/services/KngComService.svc/UpdateStudyPackageVersion",
                dataType: 'json',
                data: arr,
                cache: false,
                success: function (result) {
                    if (result) {
                        $("#divOverCourse").remove();
                        location.href = location.href;
                    }
                }
            });

            $("#btnGoHead").prop("disabled", false);
        }

        function CloseCourseView() {
            window.close();
        }

        //阻止冒泡
        function stopPropagation(e) {
            e = e || window.event;
            if (e.stopPropagation) { //W3C阻止冒泡方法  
                e.stopPropagation();
            } else {
                e.cancelBubble = true; //IE阻止冒泡方法  
            }
        }
        //显示绑定手机
        function ShowBindPhone() {
            $("#hidBindPhone").removeClass("hide");
        }

        //跳转绑定手机号码弹框
        function openBindPhone() {
            //$("#hidBindPhone").addClass("hide");

            ShowIFrameDialog(jQuery.i18n.map["mine_lbl_bindphone"], 550, 420, "/udp/Usm/Personal/Daliogs/BindMobilePhone.aspx");
            return false;
        }
    </script>
    
