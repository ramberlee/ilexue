 <script type="text/javascript">
 $(function () {
 $("#lblSiteMessageCount").text(0|0);
 //加载站内信信息
 InitMsgInfo();
 //获取通知数量
 var GetCountmethodName = 'GetWaitHandleMessage';
 var arr = {};
 AjaxAsyncCallMethod(GetCountmethodName, "",
 function (result) {
 var str = result.split("&");
 if (str.length == 2) {
 jQuery("#lblNoticCount").html(str[0]);
 document.getElementById("WaitToDoMessage").value = str[1];

 //控制待办事项闪烁
 var isShiny = "False";
 if (isShiny != "True") {
 if (str[0] > 0) {
 //待办图标闪烁
 $("#sWait").attr("class", "iconnotic_active");
 }
 }
 else {
 if (str[0] > 0) {
 var a = str[1].indexOf("@");
 var str2 = str[1].substring(a + 1);
 if (str2 > 0) {
 var addCount = str[0] - str2;
 if (addCount >= 1) {
 $("#sWait").attr("class", "iconnotic_active");
 }
 }
 }
 }
 //控制站内信图标闪烁
 var isMail = "False";
 if (isMail != "True") {
 if (0|0 > 0) {
 //站内信图标闪烁
 $("#sMail").attr("class", "iconnotic_active");
 }
 }
 else {
 var s = "0|0";
 var str = s.indexOf("|");
 if (str != -1) {
 var str2 = s.split("|");
 $("#lblSiteMessageCount").text(str2[0]);
 var addCount = str2[0] - str2[1];
 if (addCount >= 1) {
 $("#sMail").attr("class", "iconnotic_active");
 }
 else {
 $("#lblSiteMessageCount").text(0|0);
 }
 }
 }
 }
 }, true, true, false);
 setTimeout("ShowPromptMessage() ", 5000);
 });


 //执行知识搜索
 function doKnowledgeSearch() {
 var txt = new RegExp("[\\`,\\~,\\!,\\@,\\$,\\%,\\^,\\+,\\*,\\&,\\,\\/,\\?,\\|,\\:,\\<,\\>,\\{,\\},\\(,\\),\\',\\;,\\=,\"]");
 var url = "";
 var kw = jQuery.trim(jQuery("#txtKeyword").val());
 if (kw == "请输入关键字" || kw == "") {
 showWarningMsg("请输入要查找的关键字。");
 return false;
 }
 else if (kw.length < 2) {
 showWarningMsg("输入的关键字需超过一个字，请确认您输入的查询关键字。");
 return false;
 }
 else if (kw.length > 30) {
 showWarningMsg("输入的关键字最多允许30个字符，请确认您输入的查询关键字。");
 return false;
 }
 else if (txt.test(kw)) {
 showWarningMsg("输入的关键字中包含非法字符:,`~!@$%^+&*\\/?|:<>{}()[]\"。");
 return false;
 }
 else {
 url = "/search/index.htm?kw=" + escape(kw);
 }

 window.location = url;
 }

 //回车后直接搜索
 function enterToSearch(e) {
 var ev=jQuery.event.fix(e);
 if (ev.which == 13) {
 document.getElementById("btnKnowledgeSearch").click();
 //return false;
 }
 /*if (event.keyCode == 13) {
 event.keyCode = 9;
 document.getElementById("btnKnowledgeSearch").click();
 return false;
 }*/

 }

 function ShowPromptMessage() {
 var arr = '{}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/CommonService.svc/GetShowPromptMessage",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 if (result != null) {
 var str = result.split("|");
 if (str.length == 2) {
 if (str[0] != "0") {
 if (str[1] != "0") {
 var vr = "<span><a href='" + str[1] + "'>亲，您有一个新消息到达！</a></span><p><a href='" + str[1] + "'>" + str[0] + "<span>点击查看>></span></a></p>"
 jQuery("#MsgTitle").html(vr);
 }
 else {
 var vr = "<span>亲，您有一个新消息到达！</span><p>" + str[0] + "<span></span></p>"
 jQuery("#MsgTitle").html(vr);
 }
 jQuery("#ShowMessage").css("bottom", "0px").css("right", "0px").slideToggle("slow");
 }
 }
 }
 }
 });
 }

 function SetRemindTime(sender) {
 var flag = "y";
 if (jQuery(sender).prop("checked")) {
 //设置一周不提醒
 } else {
 //取消设置提醒
 flag = "n";
 }
 var arr = '{"flag":"' + flag + '"}';
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/MyIndexService.svc/SetRemindTime",
 data: arr,
 dataType: 'json',
 cache: false,
 success: function (result) {
 if (result != "ok") {
 ShowWarningMessage("设置失败。");
 }
 }
 });



 }

 function CloseShowMessage() {
 jQuery("#ShowMessage").hide();
 setTimeout("ShowPromptMessage() ", 5000);
 }

 function InitMsgInfo() {
 // 异步去取数据
 var methodName = 'GetUserSiteMessage';
 var txtType = 'SiteMessage';
 var arr = '{"type":"' + txtType + '"}';
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 var obResult = eval("(" + result + ")");
 var dvSiteMessageInfos = jQuery("#dvSiteMessageInfos");
 if (dvSiteMessageInfos.html().indexOf("正在努力加载数据，请稍候...") >= 0) {
 // 抓到数据后
 loadNoticData(dvSiteMessageInfos, obResult);
 }
 }, true, true, true);
 }


 var tmrShowSiteMsgInfos = null;
 function showSiteMsgInfos(sender, className) {
 if (tmrShowSiteMsgInfos) {
 window.clearTimeout(tmrShowSiteMsgInfos);
 }
 jQuery(sender).attr("class", className + "hover");

 jQuery(sender).one("mouseout", function () {
 tmrShowSiteMsgInfos = window.setTimeout(function () {
 jQuery(sender).attr("class", className);
 }, 500);
 });


 //站内信图标闪烁用
 var txtType01 = 'SiteMessageForMailIco';
 var arr01 = '{"key":"' + txtType01 + '"}';
 //异步保存Session
 jQuery.ajax({
 type: "POST",
 contentType: "text/json",
 url: "/Services/MyIndexService.svc/SaveSessionLoginStringForMailIco",
 data: arr01,
 dataType: 'json',
 cache: false,
 success: function (result) {
 if (result == "ok") {
 $("#sMail").attr("class", "iconnotic_new");
 }
 }
 });
 }


 //鼠标移上去显示
 function divMousehover(sender) {
 jQuery(sender).find(".timer").hide();
 jQuery(sender).find(".timer1").show();
 jQuery(sender).one("mouseout", function () {
 jQuery(this).find(".timer").show();
 jQuery(this).find(".timer1").hide();
 });
 }

 //异步把收件箱中未读的消息全部设置为已读
 //创建人：张林
 //时间：2014年5月27日12:54:05
 function UpdateIsReadByUserID() {
 var methodName = 'UpdateIsReadByUserID';
 arr = {};
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 var obResult = "";
 if (result > 0) {
 clearAllMsg();
 }
 }, false, true, true);
 }

 //全部标记已读后清空JS
 function clearAllMsg() {
 var dvSiteMessageInfos = jQuery("#dvSiteMessageInfos");
 var lblSiteMessageCount = jQuery("#lblSiteMessageCount");
 var dvNewMsgTitle = jQuery("#dvNewMsgTitle");
 divHtml = '<div class="emptyusermsg"></div><div class="emptyusertext">您暂时还没收到新的消息哦！</div>';
 dvSiteMessageInfos.html(divHtml);
 lblSiteMessageCount.html("0");
 dvNewMsgTitle.html("");
 }

 //单个删除后数量减1JS
 function clearOneMsg() {
 divHtml = '<div class="emptyusermsg"></div><div class="emptyusertext">您暂时还没收到新的消息哦！</div>';
 var dvSiteMessageInfos = jQuery("#dvSiteMessageInfos");
 var dvNewMsgTitle = jQuery("#dvNewMsgTitle");
 var lblSiteMessageCount = jQuery("#lblSiteMessageCount");
 var msgCount = jQuery("#lblSiteMessageCount").html();
 if (msgCount > 0) {
 lblSiteMessageCount.html(msgCount - 1);
 }
 else {
 dvSiteMessageInfos.html(divHtml);
 lblSiteMessageCount.html("0");
 dvNewMsgTitle.html("");
 }
 }

 function loadMsgInfo1() {
 // 异步去取数据
 var methodName = 'GetUserSingSiteMessage';
 var txtType = 'SiteMessage';
 var arr = '{"type":"' + txtType + '"}';
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 if (result == "") {
 loadNoticData1(dvSiteMessageInfos, "");
 }
 else {
 var obResult = eval("(" + result + ")");
 var dvSiteMessageInfos = jQuery("#dvSiteMessageInfos");
 if (dvSiteMessageInfos.html().indexOf("正在努力加载数据，请稍候...") >= 0) {
 // 抓到数据后

 }
 loadNoticData1(dvSiteMessageInfos, obResult[0]);
 }
 }, false, true, true);
 }

 //异步删除收件箱中的消息
 //创建人：张林
 //时间：2014年5月27日15:12:03
 function DeleteInboxMsg1(key, divId) {
 var methodName = 'DeleteInboxMsg';
 var arr = '{"key":"' + key + '"}';
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 if (result > 0) {
 jQuery('#' + divId).hide();
 clearOneMsg();
 loadMsgInfo1();
 }
 }, false, true, true);
 }

 function loadNoticData(dv, obResult) {
 var divHtml = '';
 for (var i = 0; i < obResult.length; i++) {
 divHtml += "<div id='div_" + obResult[i].ID + "' class='row clearfix' onmouseover='divMousehover(this);'><div class='floatleft' title='" + obResult[i].Title + "'><span class='iconpointpp2'>&nbsp;</span><a href='javascript:ShowMessageDetails(\"" + obResult[i].ID + "\");' >" + subStr(obResult[i].Title, 26) + "</a></div><div class='floatright color5 timer'>" + obResult[i].CreateTime + "</div><div class='floatright timer1 hiddenelement'><a href='javascript:UpdateInboxMsgIsRead(\"" + obResult[i].ID + "\",\"div_" + obResult[i].ID + "\");'>标记为已读</a>&nbsp;&nbsp;&nbsp;<a href='javascript:DeleteInboxMsg1(\"" + obResult[i].ID + "\",\"div_" + obResult[i].ID + "\");'>删除</a></div></div>";
 }
 if (divHtml == '') {
 jQuery("#dvNewMsgTitle").hide();
 divHtml = '<div class="emptyusermsg"></div><div class="emptyusertext">您暂时还没收到新的消息哦！</div>';
 } jQuery(dv).html(divHtml);
 }

 function loadNoticData1(dv, obResult) {
 var divHtml = '';
 if (obResult) {
 divHtml += "<div id='div_" + obResult.ID + "' class='row clearfix' onmouseover='divMousehover(this);'><div class='floatleft' title='" + obResult.Title + "'><span class='iconpointpp2'>&nbsp;</span><a href='javascript:ShowMessageDetails(\"" + obResult.ID + "\");' >" + subStr(obResult.Title, 26) + "</a></div><div class='floatright color5 timer'>" + obResult.CreateTime + "</div><div class='floatright timer1 hiddenelement'><a href='javascript:UpdateInboxMsgIsRead(\"" + obResult.ID + "\",\"div_" + obResult.ID + "\");'>标记为已读</a>&nbsp;&nbsp;&nbsp;<a href='javascript:DeleteInboxMsg1(\"" + obResult.ID + "\",\"div_" + obResult.ID + "\");'>删除</a></div></div>";
 if (divHtml == '') {
 jQuery("#dvNewMsgTitle").hide();
 divHtml = '<div class="emptyusermsg"></div><div class="emptyusertext">您暂时还没收到新的消息哦！</div>';
 }
 jQuery(dv).append(divHtml);
 }
 }

 //更新收件箱消息为已读
 //创建人：张林
 //时间：2014年5月27日15:35:21
 function UpdateInboxMsgIsRead(key, divId) {
 var methodName = 'UpdateInboxMsgIsRead';
 var arr = '{"key":"' + key + '"}';
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 if (result > 0) {
 jQuery('#' + divId).hide();
 clearOneMsg();
 loadMsgInfo1();
 }
 }, false, true, true);
 }

 function subStr(txt, index) {
 var txtSub = txt;
 if (index > 0 && txt.length > 26) {
 txtSub = txt.substring(0, 26) + "...";
 }
 return txtSub;
 }

 function ShowMessageDetails(id) {
 var url = "/message/messagedetail.htm";
 if (url.indexOf("?") > -1) {
 url += "&";
 }
 else {
 url += "?";
 }
 window.location.href = url + 'ID=' + id + '&OP=Edit&MsgSourceType=InBox';
 }

 //用户账号信息
 function changeHoverStyleForHidDiv(sender, className) {
 if (tmrShowSiteMsgInfos) {
 window.clearTimeout(tmrShowSiteMsgInfos);
 }
 jQuery(".msgareahover").attr("class", "msgarea");
 jQuery(sender).attr("class", className + "hover");
 //异步去取用户账户相关信息
 var methodName = 'GetUserClassLevelInfo';
 arr = {};
 AjaxAsyncCallMethod(methodName, arr,
 function (result) {
 var obResult = eval("(" + result + ")");
 var dvAccountTeacher = jQuery("#dvAccountTeacher");
 // 抓到数据后
 loadTeacherData(dvAccountTeacher, obResult);
 }, false, true, true);
 jQuery(sender).one("mouseout", function () {
 jQuery(this).attr("class", className);
 });
 }

 //动态加载到页面
 function loadTeacherData(dv, obResult) {
 var divHtml = "";
 if (obResult != null) {
 divHtml = "<div class='pic'><img alt=" + obResult[0].CnName + " src=" + obResult[0].HeadPictureUrl + "></div><div class='info'><div class='row color3'>" + obResult[0].CnName + "<span class='color5'>&nbsp;" + obResult[0].UserName + "</span></div><div class='row color4 '><img alt=" + obResult[0].UserClassLevelName + " src=" + obResult[0].UserClassLevelImageUrl + " height='9'>&nbsp;&nbsp;" + obResult[0].UserClassLevelName + "</div><div class='row'><span class='color5'>" + obResult[0].AttentionCount + "</span>&nbsp;<span class='color4'>个关注</span>&nbsp;&nbsp;&nbsp;&nbsp;<span style='color:#BBBBBB;'>|&nbsp;&nbsp;</span><span class='color5'>" + obResult[0].FansCount + "</span>&nbsp;<span class='color4'>个粉丝</span>&nbsp;&nbsp;&nbsp;&nbsp;<span style='color:#BBBBBB;'>|&nbsp;&nbsp;</span><span class='color5'>" + obResult[0].TotalUserPoints + "</span>&nbsp;<span class='color4'>个积分</span></div></div>";
 }
 jQuery(dv).html(divHtml);
 }

 </script>
